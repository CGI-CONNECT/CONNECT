<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-AdapterPEPService" depends="wsimport-init">
        <condition property="wsimport-service-AdapterPEPService.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adapterpep/AdapterPEP.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterPEPService" depends="wsimport-init,wsimport-service-check-AdapterPEPService" unless="wsimport-service-AdapterPEPService.notRequired">
        <property name="service-wsdl-AdapterPEPService" location="${meta.inf}/xml-resources/web-services/AdapterPEPService/wsdl/AdapterPEP.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterPEPService}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterPEPService" location="${meta.inf}/wsdl/AdapterPEPService"/>
        <property name="sourceLocation-AdapterPEPService" location="${meta.inf}/xml-resources/web-services/AdapterPEPService/wsdl/"/>
        <copy todir="${targetLocation-AdapterPEPService}">
            <fileset dir="${sourceLocation-AdapterPEPService}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterPEPService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adapterpep"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterPEPService"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
