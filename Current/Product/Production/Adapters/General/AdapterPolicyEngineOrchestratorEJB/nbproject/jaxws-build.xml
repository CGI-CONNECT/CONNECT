<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-AdapterPolicyEngineOrchestrator" depends="wsimport-init">
        <condition property="wsimport-service-AdapterPolicyEngineOrchestrator.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adapterpolicyengineorchestrator/AdapterPolicyEngineOrchestrator.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterPolicyEngineOrchestrator" depends="wsimport-init,wsimport-service-check-AdapterPolicyEngineOrchestrator" unless="wsimport-service-AdapterPolicyEngineOrchestrator.notRequired">
        <property name="service-wsdl-AdapterPolicyEngineOrchestrator" location="${meta.inf}/xml-resources/web-services/AdapterPolicyEngineOrchestrator/wsdl/AdapterPolicyEngineOrchestrator.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterPolicyEngineOrchestrator}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterPolicyEngineOrchestrator" location="${meta.inf}/wsdl/AdapterPolicyEngineOrchestrator"/>
        <property name="sourceLocation-AdapterPolicyEngineOrchestrator" location="${meta.inf}/xml-resources/web-services/AdapterPolicyEngineOrchestrator/wsdl/"/>
        <copy todir="${targetLocation-AdapterPolicyEngineOrchestrator}">
            <fileset dir="${sourceLocation-AdapterPolicyEngineOrchestrator}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterPolicyEngineOrchestrator" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adapterpolicyengineorchestrator"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterPolicyEngineOrchestrator"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
