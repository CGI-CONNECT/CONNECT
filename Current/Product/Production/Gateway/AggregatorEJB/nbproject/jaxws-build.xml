<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-NhincComponentAggregator" depends="wsimport-init">
        <condition property="wsimport-service-NhincComponentAggregator.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhinccomponentaggregator/NhincComponentAggregator.java"/>
        </condition>
    </target>
    <target name="wsimport-service-NhincComponentAggregator" depends="wsimport-init,wsimport-service-check-NhincComponentAggregator" unless="wsimport-service-NhincComponentAggregator.notRequired">
        <property name="service-wsdl-NhincComponentAggregator" location="${meta.inf}/xml-resources/web-services/NhincComponentAggregator/wsdl/NhincComponentAggregator.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-NhincComponentAggregator}" catalog="catalog.xml"/>
        <property name="targetLocation-NhincComponentAggregator" location="${meta.inf}/wsdl/NhincComponentAggregator"/>
        <property name="sourceLocation-NhincComponentAggregator" location="${meta.inf}/xml-resources/web-services/NhincComponentAggregator/wsdl/"/>
        <copy todir="${targetLocation-NhincComponentAggregator}">
            <fileset dir="${sourceLocation-NhincComponentAggregator}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincComponentAggregator" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhinccomponentaggregator"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-NhincComponentAggregator"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
