<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-PolicyEngineTransformationService" depends="wsimport-init">
        <condition property="wsimport-service-PolicyEngineTransformationService.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincinternalcomponentpolicyenginetransform/NhincComponentInternalPolicyEngineTransformService.java"/>
        </condition>
    </target>
    <target name="wsimport-service-PolicyEngineTransformationService" depends="wsimport-init,wsimport-service-check-PolicyEngineTransformationService" unless="wsimport-service-PolicyEngineTransformationService.notRequired">
        <property name="service-wsdl-PolicyEngineTransformationService" location="${meta.inf}/xml-resources/web-services/PolicyEngineTransformationService/wsdl/NhincComponentInternalPolicyEngineTransform.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-PolicyEngineTransformationService}" catalog="catalog.xml"/>
        <property name="targetLocation-PolicyEngineTransformationService" location="${meta.inf}/wsdl/PolicyEngineTransformationService"/>
        <property name="sourceLocation-PolicyEngineTransformationService" location="${meta.inf}/xml-resources/web-services/PolicyEngineTransformationService/wsdl/"/>
        <copy todir="${targetLocation-PolicyEngineTransformationService}">
            <fileset dir="${sourceLocation-PolicyEngineTransformationService}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-PolicyEngineTransformationService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincinternalcomponentpolicyenginetransform"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-PolicyEngineTransformationService"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
