<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-check-SamlReceive" depends="wsimport-init">
        <condition property="wsimport-client-SamlReceive.notRequired">
            <available file="${build.generated.dir}/wsimport/client/org/netbeans/j2ee/wsdl/interfaces/samlreceive" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-client-SamlReceive" depends="wsimport-init,wsimport-client-check-SamlReceive" unless="wsimport-client-SamlReceive.notRequired">
        <wsimport xendorsed="true" sourcedestdir="${build.generated.dir}/wsimport/client" extension="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${meta.inf}/xml-resources/web-service-references/SamlReceive/wsdl/SamlReceive.wsdl" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Examples/SAMLCommunicationExample/Interfaces/src/wsdl/SamlReceive.wsdl" catalog="catalog.xml"/>
        <copy todir="${classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-client-clean-SamlReceive" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/org/netbeans/j2ee/wsdl/interfaces/samlreceive"/>
    </target>
    <target name="wsimport-service-check-AgencyProcessWS" depends="wsimport-init">
        <condition property="wsimport-service-AgencyProcessWS.notRequired">
            <available file="${build.generated.dir}/wsimport/service/org/netbeans/j2ee/wsdl/interfaces/samlsend" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-service-AgencyProcessWS" depends="wsimport-init,wsimport-service-check-AgencyProcessWS" unless="wsimport-service-AgencyProcessWS.notRequired">
        <wsimport xendorsed="true" sourcedestdir="${build.generated.dir}/wsimport/service" extension="true" verbose="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${meta.inf}/xml-resources/web-services/AgencyProcessWS/wsdl/SamlSend.wsdl" catalog="catalog.xml"/>
        <copy todir="${basedir}/${meta.inf}/wsdl/AgencyProcessWS">
            <fileset dir="${basedir}/${meta.inf}/xml-resources/web-services/AgencyProcessWS/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AgencyProcessWS" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/org/netbeans/j2ee/wsdl/interfaces/samlsend"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-SamlReceive"/>
    <target name="wsimport-client-compile" depends="wsimport-client-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AgencyProcessWS"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
