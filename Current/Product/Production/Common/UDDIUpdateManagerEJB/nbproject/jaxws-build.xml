<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-check-uddi_v3_service" depends="wsimport-init">
        <condition property="wsimport-client-uddi_v3_service.notRequired">
            <available file="${build.generated.dir}/wsimport/client/uddi-org:v3_service/uddi_v3_service.java"/>
        </condition>
    </target>
    <target name="wsimport-client-uddi_v3_service" depends="wsimport-init,wsimport-client-check-uddi_v3_service" unless="wsimport-client-uddi_v3_service.notRequired">
        <property name="wsdl-uddi_v3_service" location="${meta.inf}/xml-resources/web-service-references/uddi_v3_service/wsdl/uddi_v3_service.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${wsdl-uddi_v3_service}" catalog="catalog.xml"/>
    </target>
    <target name="wsimport-client-clean-uddi_v3_service" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/uddi-org:v3_service"/>
    </target>
    <target name="wsimport-service-check-UDDIUpdateManager" depends="wsimport-init">
        <condition property="wsimport-service-UDDIUpdateManager.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhinccomponentuddiupdatemanager/NhincComponentUDDIUpdateManager.java"/>
        </condition>
    </target>
    <target name="wsimport-service-UDDIUpdateManager" depends="wsimport-init,wsimport-service-check-UDDIUpdateManager" unless="wsimport-service-UDDIUpdateManager.notRequired">
        <property name="service-wsdl-UDDIUpdateManager" location="${meta.inf}/xml-resources/web-services/UDDIUpdateManager/wsdl/NhincComponentUDDIUpdateManager.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-UDDIUpdateManager}" catalog="catalog.xml"/>
        <property name="targetLocation-UDDIUpdateManager" location="${meta.inf}/wsdl/UDDIUpdateManager"/>
        <property name="sourceLocation-UDDIUpdateManager" location="${meta.inf}/xml-resources/web-services/UDDIUpdateManager/wsdl/"/>
        <copy todir="${targetLocation-UDDIUpdateManager}">
            <fileset dir="${sourceLocation-UDDIUpdateManager}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-UDDIUpdateManager" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhinccomponentuddiupdatemanager"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-uddi_v3_service"/>
    <target name="wsimport-client-compile" depends="wsimport-client-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
        <copy todir="${classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-UDDIUpdateManager"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
