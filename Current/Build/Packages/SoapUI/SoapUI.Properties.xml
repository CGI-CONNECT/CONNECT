	<property name="AntHome"  value="${environment::get-variable('ANT_HOME')}" overwrite="false"/>
	<property name="JavaHome" value="${environment::get-variable('JAVA_HOME')}" overwrite="false"/>
<!--	<property name="NetBeansHome" value="C:\Java\NetBeans6.0.1\NetBeans 6.0.1" overwrite="false"/> -->
	<property name="Path" value="${environment::get-variable('PATH')}" overwrite="false"/>
	
