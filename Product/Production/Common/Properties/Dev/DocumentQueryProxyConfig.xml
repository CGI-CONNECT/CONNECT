<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

<!-- 
	The beans available in this file must be identified in the "description" element for the Spring configuration application. Beans are listed between braces 
	in the description separated with a comma like the following sample: {somebean,anotherbean}
-->
	<description>Beans included in this file: {nhindocquery,entitydocquery,passthrudocquery,adapterdocquery, adapterdocumentregistry}</description>

	<!-- nhindocquery -->
	<!-- No-op Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.nhin.proxy.NhinDocQueryProxyNoOpImpl" id="nhindocquerynoop" name="nhindocquerynoop"> 
		<meta key="impltype" value="noop"/>
	</bean> 

	<!-- Secured web service Implementation -->
    <bean class="gov.hhs.fha.nhinc.docquery.nhin.proxy.NhinDocQueryProxyWebServiceSecuredImpl" id="nhindocquery" name="nhindocquerywssecured"> 
		<meta key="impltype" value="wssecured"/>
		<meta key="default" value="true"/>
	</bean> 

	<!-- entitydocquery -->
	<!-- No-op Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.entity.proxy.EntityDocQueryProxyNoOpImpl" id="entitydocquerynoop" name="entitydocquerynoop"> 
		<meta key="impltype" value="noop"/>
	</bean> 

	<!-- Java Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.entity.proxy.EntityDocQueryProxyJavaImpl" id="entitydocqueryjava" name="entitydocqueryjava"> 
		<meta key="impltype" value="java"/>
	</bean> 

	<!-- Secured web service Implementation -->
    <bean class="gov.hhs.fha.nhinc.docquery.entity.proxy.EntityDocQueryProxyWebServiceSecuredImpl" id="entitydocquery" name="entitydocquerywssecured"> 
		<meta key="impltype" value="wssecured"/>
		<meta key="default" value="true"/>
	</bean> 

	<!-- Unsecured web service Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.entity.proxy.EntityDocQueryProxyWebServiceUnsecuredImpl" id="entitydocquerywsunsecured" name="entitydocquerywsunsecured"> 
		<meta key="impltype" value="wsunsecured"/>
	</bean> 

	<!-- passthrudocquery -->
	<!-- No-op Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.passthru.proxy.PassthruDocQueryProxyNoOpImpl" id="passthrudocquerynoop" name="passthrudocquerynoop"> 
		<meta key="impltype" value="noop"/>
	</bean> 

	<!-- Java Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.passthru.proxy.PassthruDocQueryProxyJavaImpl" id="passthrudocquery" name="passthrudocqueryjava"> 
		<meta key="impltype" value="java"/>
		<meta key="default" value="true"/>
	</bean> 

	<!-- Secured web service Implementation -->
    <bean class="gov.hhs.fha.nhinc.docquery.passthru.proxy.PassthruDocQueryProxyWebServiceSecuredImpl" id="passthrudocquerywssecured" name="passthrudocquerywssecured"> 
		<meta key="impltype" value="wssecured"/>
	</bean> 

	<!-- Unsecured web service Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.passthru.proxy.PassthruDocQueryProxyWebServiceUnsecuredImpl" id="passthrudocquerywsunsecured" name="passthrudocquerywsunsecured"> 
		<meta key="impltype" value="wsunsecured"/>
	</bean>
	
    <!-- Beans defined : Adapter interface -->
	
    <!-- No-op Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.adapter.proxy.AdapterDocQueryProxyNoOpImpl" id="adapterdocquerynoop" name="adapterdocquerynoop"> 
		<meta key="impltype" value="noop"/>
	</bean> 

	<!-- Java Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.adapter.proxy.AdapterDocQueryProxyJavaImpl" id="adapterdocqueryjava" name="adapterdocqueryjava"> 
		<meta key="impltype" value="java"/>
	</bean> 

	<!-- Secured web service Implementation -->
    <bean class="gov.hhs.fha.nhinc.docquery.adapter.proxy.AdapterDocQueryProxyWebServiceSecuredImpl" id="adapterdocquery" name="adapterdocquerywssecured"> 
		<meta key="impltype" value="wssecured"/>
		<meta key="default" value="true"/>
	</bean> 

	<!-- Unsecured web service Implementation -->
	<bean class="gov.hhs.fha.nhinc.docquery.adapter.proxy.AdapterDocQueryProxyWebServiceUnsecuredImpl" id="adapterdocquerywsunsecured" name="adapterdocquerywsunsecured"> 
		<meta key="impltype" value="wsunsecured"/>
	</bean>
	
	<!-- Beans defined: Adapter DocRegistry-->

    <!-- No-op Implementation -->
	<bean class="gov.hhs.fha.nhinc.docregistry.adapter.proxy.AdapterComponentDocRegistryProxyNoOpImpl" id="adapterdocumentregistrynoop" name="adapterdocumentregistrynoop"> 
		<meta key="impltype" value="noop"/>
	</bean> 

	<!-- Java Implementation -->
	<bean class="gov.hhs.fha.nhinc.docregistry.adapter.proxy.AdapterComponentDocRegistryProxyJavaImpl" id="adapterdocumentregistryjava" name="adapterdocumentregistryjava"> 
		<meta key="impltype" value="java"/>
	</bean> 

	<!-- Unsecured web service Implementation -->
	<bean class="gov.hhs.fha.nhinc.docregistry.adapter.proxy.AdapterComponentDocRegistryProxyWebServiceUnsecuredImpl" id="adapterdocumentregistry" name="adapterdocumentregistrywsunsecured"> 
		<meta key="impltype" value="wsunsecured"/>
		<meta key="default" value="true"/>
	</bean> 	
	

</beans>
