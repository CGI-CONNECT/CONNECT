<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-uddi_v3_service" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/uddi_v3_service"/>
        <property name="wsdl-uddi_v3_service" location="${meta.inf}/xml-resources/web-service-references/uddi_v3_service/wsdl/uddi_v3_service.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/uddi_v3_service" destdir="${build.generated.dir}/jax-wsCache/uddi_v3_service" wsdl="${wsdl-uddi_v3_service}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/uddi_v3_service">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-uddi_v3_service" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/uddi_v3_service"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/uddi-org:v3_service"/>
    </target>
    <target name="wsimport-service-UDDIUpdateManager" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/UDDIUpdateManager"/>
        <property name="service-wsdl-UDDIUpdateManager" location="${meta.inf}/xml-resources/web-services/UDDIUpdateManager/wsdl/NhincComponentUDDIUpdateManager.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/UDDIUpdateManager" destdir="${build.generated.dir}/jax-wsCache/service/UDDIUpdateManager" wsdl="${service-wsdl-UDDIUpdateManager}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/UDDIUpdateManager">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-UDDIUpdateManager" location="${meta.inf}/wsdl/UDDIUpdateManager"/>
        <property name="sourceLocation-UDDIUpdateManager" location="${meta.inf}/xml-resources/web-services/UDDIUpdateManager/wsdl/"/>
        <copy todir="${targetLocation-UDDIUpdateManager}">
            <fileset dir="${sourceLocation-UDDIUpdateManager}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-UDDIUpdateManager" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/UDDIUpdateManager"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhinccomponentuddiupdatemanager"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-uddi_v3_service"/>
    <target name="wsimport-service-generate" depends="wsimport-service-UDDIUpdateManager"/>
</project>
