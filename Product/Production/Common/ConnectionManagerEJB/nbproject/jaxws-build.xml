<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-NhincComponentConnectionManager" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/NhincComponentConnectionManager"/>
        <property name="service-wsdl-NhincComponentConnectionManager" location="${meta.inf}/xml-resources/web-services/NhincComponentConnectionManager/wsdl/NhincComponentConnectionManager.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/NhincComponentConnectionManager" destdir="${build.generated.dir}/jax-wsCache/service/NhincComponentConnectionManager" wsdl="${service-wsdl-NhincComponentConnectionManager}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/NhincComponentConnectionManager">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-NhincComponentConnectionManager" location="${meta.inf}/wsdl/NhincComponentConnectionManager"/>
        <property name="sourceLocation-NhincComponentConnectionManager" location="${meta.inf}/xml-resources/web-services/NhincComponentConnectionManager/wsdl/"/>
        <copy todir="${targetLocation-NhincComponentConnectionManager}">
            <fileset dir="${sourceLocation-NhincComponentConnectionManager}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincComponentConnectionManager" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/NhincComponentConnectionManager"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhinccomponentconnectionmanager"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-NhincComponentConnectionManager"/>
</project>
