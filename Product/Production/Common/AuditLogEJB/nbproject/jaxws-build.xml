    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-NhincComponentAuditRepository" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/NhincComponentAuditRepository"/>
        <property name="wsdl-NhincComponentAuditRepository" location="${meta.inf}/xml-resources/web-service-references/NhincComponentAuditRepository/wsdl/NhincComponentAuditRepository.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/NhincComponentAuditRepository" destdir="${build.generated.dir}/jax-wsCache/NhincComponentAuditRepository" wsdl="${wsdl-NhincComponentAuditRepository}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/NhincComponentAuditRepository.wsdl" xnocompile="true">
            <depends file="${wsdl-NhincComponentAuditRepository}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/NhincComponentAuditRepository"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/NhincComponentAuditRepository">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-NhincComponentAuditRepository" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/NhincComponentAuditRepository"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov:hhs:fha:nhinc:nhinccomponentauditrepository"/>
    </target>
    <target name="wsimport-service-AuditLog" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/AuditLog"/>
        <property name="service-wsdl-AuditLog" location="${meta.inf}/xml-resources/web-services/AuditLog/wsdl/NhincComponentInternalAuditRepository.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/AuditLog" destdir="${build.generated.dir}/jax-wsCache/service/AuditLog" wsdl="${service-wsdl-AuditLog}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-AuditLog}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/AuditLog"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/AuditLog">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-AuditLog" location="${meta.inf}/wsdl/AuditLog"/>
        <property name="sourceLocation-AuditLog" location="${meta.inf}/xml-resources/web-services/AuditLog/wsdl/"/>
        <copy todir="${targetLocation-AuditLog}">
            <fileset dir="${sourceLocation-AuditLog}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AuditLog" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/AuditLog"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhinccomponentinternalauditrepository"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-NhincComponentAuditRepository"/>
    <target name="wsimport-service-generate" depends="wsimport-service-AuditLog"/>
</project>
