<?xml version="1.0" encoding="UTF-8"?>
<definitions name="EntityComponentInternalNotifyOrch" targetNamespace="urn:gov:hhs:fha:nhinc:entitycomponentinternalnotifyorch"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
    xmlns:tns="urn:gov:hhs:fha:nhinc:entitycomponentinternalnotifyorch" 
    xmlns:nccommonentity="urn:gov:hhs:fha:nhinc:common:nhinccommonentity" 
    xmlns:nccommon="urn:gov:hhs:fha:nhinc:common:nhinccommon" 
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype">
    <types>
        <xsd:schema targetNamespace="urn:gov:hhs:fha:nhinc:entitycomponentinternalnotifyorch">
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonentity" schemaLocation="../schemas/nhinc/common/NhincCommonEntity.xsd"/>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommon" schemaLocation="../schemas/nhinc/common/NhincCommon.xsd"/>
        </xsd:schema>
    </types>
    <message name="DocumentNotifyRequestMessage">
        <part name="DocumentNotifyRequest" element="nccommonentity:NotifySubscribersOfDocumentRequest"/>
    </message>
    <message name="DocumentNotifyResponseMessage">
        <part name="DocumentNotifyResponse" element="nccommon:Acknowledgement"/>
    </message>
    <portType name="EntityComponentInternalNotifyOrchPortType">
        <operation name="DocumentNotify">
            <input name="DocumentNotifyRequest" message="tns:DocumentNotifyRequestMessage"/>
            <output name="DocumentNotifyResponse" message="tns:DocumentNotifyResponseMessage"/>
        </operation>
    </portType>
    <wsdl:binding name="EntityComponentInternalNotifyOrchBindingSoap11" 
                type="tns:EntityComponentInternalNotifyOrchPortType">
        <soap:binding style="document" 
                  transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="DocumentNotify">
            <soap:operation soapAction="urn:DocumentNotify"/>
            <wsdl:input name="DocumentNotifyRequest">
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output name="DocumentNotifyResponse">
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    
    <wsdl:service name="EntityComponentInternalNotifyOrchService">
        <wsdl:port name="EntityComponentInternalNotifyOrchPortSoap11" 
               binding="tns:EntityComponentInternalNotifyOrchBindingSoap11">
            <soap:address 
         location=
            "http://localhost:${HttpDefaultPort}/NhinConnect/EntityComponentInternalNotifyOrch"/>
        </wsdl:port>
    </wsdl:service>
    <plnk:partnerLinkType name="EntityComponentInternalNotifyOrch">
        <!-- A partner link type is automatically generated when a new port type is added. Partner link types are used by BPEL processes. 
In a BPEL process, a partner link represents the interaction between the BPEL process and a partner service. Each partner link is associated with a partner link type.
A partner link type characterizes the conversational relationship between two services. The partner link type can have one or two roles.-->
        <plnk:role name="EntityComponentInternalNotifyOrchPortTypeRole" portType="tns:EntityComponentInternalNotifyOrchPortType"/>
    </plnk:partnerLinkType>
</definitions>
