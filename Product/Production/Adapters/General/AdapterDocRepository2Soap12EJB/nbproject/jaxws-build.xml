<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-AdapterDocRepository2Soap12Service" depends="wsimport-init">
        <condition property="wsimport-service-AdapterDocRepository2Soap12Service.notRequired">
            <available file="${build.generated.dir}/wsimport/service/ihe/iti/xds_b/_2007/DocumentRepository_Service.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterDocRepository2Soap12Service" depends="wsimport-init,wsimport-service-check-AdapterDocRepository2Soap12Service" unless="wsimport-service-AdapterDocRepository2Soap12Service.notRequired">
        <property name="service-wsdl-AdapterDocRepository2Soap12Service" location="${meta.inf}/xml-resources/web-services/AdapterDocRepository2Soap12Service/wsdl/AdapterComponentDocRepository.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterDocRepository2Soap12Service}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterDocRepository2Soap12Service" location="${meta.inf}/wsdl/AdapterDocRepository2Soap12Service"/>
        <property name="sourceLocation-AdapterDocRepository2Soap12Service" location="${meta.inf}/xml-resources/web-services/AdapterDocRepository2Soap12Service/wsdl/"/>
        <copy todir="${targetLocation-AdapterDocRepository2Soap12Service}">
            <fileset dir="${sourceLocation-AdapterDocRepository2Soap12Service}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterDocRepository2Soap12Service" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/ihe/iti/xds_b/_2007"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterDocRepository2Soap12Service"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
