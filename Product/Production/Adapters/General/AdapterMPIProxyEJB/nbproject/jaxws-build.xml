<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-AdapterMpiSecured" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/AdapterMpiSecured"/>
        <property name="wsdl-AdapterMpiSecured" location="${meta.inf}/xml-resources/web-service-references/AdapterMpiSecured/wsdl/AdapterMpiSecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/AdapterMpiSecured" destdir="${build.generated.dir}/jax-wsCache/AdapterMpiSecured" wsdl="${wsdl-AdapterMpiSecured}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/AdapterMpiSecured.wsdl" xnocompile="true">
            <depends file="${wsdl-AdapterMpiSecured}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/AdapterMpiSecured"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/AdapterMpiSecured">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-AdapterMpiSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/AdapterMpiSecured"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov:hhs:fha:nhinc:adaptermpi"/>
    </target>
    <target name="wsimport-service-AdapterMpi" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/AdapterMpi"/>
        <property name="service-wsdl-AdapterMpi" location="${meta.inf}/xml-resources/web-services/AdapterMpi/wsdl/AdapterMpi.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/AdapterMpi" destdir="${build.generated.dir}/jax-wsCache/service/AdapterMpi" wsdl="${service-wsdl-AdapterMpi}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-AdapterMpi}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/AdapterMpi"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/AdapterMpi">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-AdapterMpi" location="${meta.inf}/wsdl/AdapterMpi"/>
        <property name="sourceLocation-AdapterMpi" location="${meta.inf}/xml-resources/web-services/AdapterMpi/wsdl/"/>
        <copy todir="${targetLocation-AdapterMpi}">
            <fileset dir="${sourceLocation-AdapterMpi}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterMpi" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/AdapterMpi"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/adaptermpi"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-AdapterMpiSecured"/>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterMpi"/>
</project>
