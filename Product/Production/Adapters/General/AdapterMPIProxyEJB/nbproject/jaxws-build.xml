<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-check-AdapterMpiSecured" depends="wsimport-init">
        <condition property="wsimport-client-AdapterMpiSecured.notRequired">
            <available file="${build.generated.dir}/wsimport/client/gov:hhs:fha:nhinc:adaptermpi/AdapterMpiSecured.java"/>
        </condition>
    </target>
    <target name="wsimport-client-AdapterMpiSecured" depends="wsimport-init,wsimport-client-check-AdapterMpiSecured" unless="wsimport-client-AdapterMpiSecured.notRequired">
        <property name="wsdl-AdapterMpiSecured" location="${meta.inf}/xml-resources/web-service-references/AdapterMpiSecured/wsdl/AdapterMpiSecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${wsdl-AdapterMpiSecured}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/AdapterMpiSecured.wsdl" xnocompile="true"/>
    </target>
    <target name="wsimport-client-clean-AdapterMpiSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/gov:hhs:fha:nhinc:adaptermpi"/>
    </target>
    <target name="wsimport-service-check-AdapterMpi" depends="wsimport-init">
        <condition property="wsimport-service-AdapterMpi.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptermpi/AdapterMpiService.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterMpi" depends="wsimport-init,wsimport-service-check-AdapterMpi" unless="wsimport-service-AdapterMpi.notRequired">
        <property name="service-wsdl-AdapterMpi" location="${meta.inf}/xml-resources/web-services/AdapterMpi/wsdl/AdapterMpi.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterMpi}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterMpi" location="${meta.inf}/wsdl/AdapterMpi"/>
        <property name="sourceLocation-AdapterMpi" location="${meta.inf}/xml-resources/web-services/AdapterMpi/wsdl/"/>
        <copy todir="${targetLocation-AdapterMpi}">
            <fileset dir="${sourceLocation-AdapterMpi}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterMpi" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptermpi"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-AdapterMpiSecured"/>
    <target name="wsimport-client-compile" depends="wsimport-client-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
        <copy todir="${classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterMpi"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
