<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-AdapterPolicyEngineOrchestrator" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineOrchestrator"/>
        <property name="service-wsdl-AdapterPolicyEngineOrchestrator" location="${meta.inf}/xml-resources/web-services/AdapterPolicyEngineOrchestrator/wsdl/AdapterPolicyEngineOrchestrator.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineOrchestrator" destdir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineOrchestrator" wsdl="${service-wsdl-AdapterPolicyEngineOrchestrator}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-AdapterPolicyEngineOrchestrator}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineOrchestrator"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineOrchestrator">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-AdapterPolicyEngineOrchestrator" location="${meta.inf}/wsdl/AdapterPolicyEngineOrchestrator"/>
        <property name="sourceLocation-AdapterPolicyEngineOrchestrator" location="${meta.inf}/xml-resources/web-services/AdapterPolicyEngineOrchestrator/wsdl/"/>
        <copy todir="${targetLocation-AdapterPolicyEngineOrchestrator}">
            <fileset dir="${sourceLocation-AdapterPolicyEngineOrchestrator}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterPolicyEngineOrchestrator" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineOrchestrator"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/adapterpolicyengineorchestrator"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterPolicyEngineOrchestrator"/>
</project>
