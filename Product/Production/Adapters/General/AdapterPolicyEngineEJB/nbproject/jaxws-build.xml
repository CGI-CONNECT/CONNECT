<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-AdapterPolicyEngineSecured" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineSecured"/>
        <property name="service-wsdl-AdapterPolicyEngineSecured" location="${meta.inf}/xml-resources/web-services/AdapterPolicyEngineSecured/wsdl/AdapterPolicyEngineSecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineSecured" destdir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineSecured" wsdl="${service-wsdl-AdapterPolicyEngineSecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-AdapterPolicyEngineSecured}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineSecured"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineSecured">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-AdapterPolicyEngineSecured" location="${meta.inf}/wsdl/AdapterPolicyEngineSecured"/>
        <property name="sourceLocation-AdapterPolicyEngineSecured" location="${meta.inf}/xml-resources/web-services/AdapterPolicyEngineSecured/wsdl/"/>
        <copy todir="${targetLocation-AdapterPolicyEngineSecured}">
            <fileset dir="${sourceLocation-AdapterPolicyEngineSecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterPolicyEngineSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/AdapterPolicyEngineSecured"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/adapterpolicyenginesecured"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterPolicyEngineSecured"/>
</project>
