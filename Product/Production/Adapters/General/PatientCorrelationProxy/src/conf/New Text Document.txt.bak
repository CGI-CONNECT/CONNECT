<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 EJB 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_0-0.dtd">
<sun-ejb-jar>
    <enterprise-beans>
        <ejb>
            <ejb-name>PatientCorrelationService</ejb-name>
            <webservice-endpoint>
                <port-component-name>PatientCorrelationService</port-component-name>
                <endpoint-address-uri>/PatientCorrelationService</endpoint-address-uri>
            </webservice-endpoint>   
        </ejb>
        <ejb>
            <ejb-name>PatientCorrelationSecuredService</ejb-name>
            <webservice-endpoint>
                <port-component-name>PatientCorrelationSecuredService</port-component-name>
                <endpoint-address-uri>/PatientCorrelationSecuredService</endpoint-address-uri>
                
                <login-config>
				   				<auth-method>CLIENT-CERT</auth-method>
				   				<realm>certificate</realm>
								</login-config>
								<transport-guarantee>CONFIDENTIAL</transport-guarantee> 
            </webservice-endpoint>   
        </ejb>        
    </enterprise-beans>
</sun-ejb-jar>