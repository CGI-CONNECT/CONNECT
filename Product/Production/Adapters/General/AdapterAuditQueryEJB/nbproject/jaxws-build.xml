<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-AdapterAuditQuery" depends="wsimport-init">
        <condition property="wsimport-service-AdapterAuditQuery.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adapterauditlogquery/AdapterAuditLogQuery.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterAuditQuery" depends="wsimport-init,wsimport-service-check-AdapterAuditQuery" unless="wsimport-service-AdapterAuditQuery.notRequired">
        <property name="service-wsdl-AdapterAuditQuery" location="${meta.inf}/xml-resources/web-services/AdapterAuditQuery/wsdl/AdapterAuditLogQuery.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterAuditQuery}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterAuditQuery" location="${meta.inf}/wsdl/AdapterAuditQuery"/>
        <property name="sourceLocation-AdapterAuditQuery" location="${meta.inf}/xml-resources/web-services/AdapterAuditQuery/wsdl/"/>
        <copy todir="${targetLocation-AdapterAuditQuery}">
            <fileset dir="${sourceLocation-AdapterAuditQuery}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterAuditQuery" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adapterauditlogquery"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterAuditQuery"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
