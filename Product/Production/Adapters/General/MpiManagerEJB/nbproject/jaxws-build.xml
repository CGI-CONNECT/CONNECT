<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-MpiManager" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/MpiManager"/>
        <property name="service-wsdl-MpiManager" location="${meta.inf}/xml-resources/web-services/MpiManager/wsdl/AdapterMpiManager.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/MpiManager" destdir="${build.generated.dir}/jax-wsCache/service/MpiManager" wsdl="${service-wsdl-MpiManager}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/MpiManager">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-MpiManager" location="${meta.inf}/wsdl/MpiManager"/>
        <property name="sourceLocation-MpiManager" location="${meta.inf}/xml-resources/web-services/MpiManager/wsdl/"/>
        <copy todir="${targetLocation-MpiManager}">
            <fileset dir="${sourceLocation-MpiManager}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-MpiManager" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/MpiManager"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/adaptermpimanager"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-MpiManager"/>
</project>
