<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-AdapterSubjectDiscoverySecured" depends="wsimport-init">
        <condition property="wsimport-service-AdapterSubjectDiscoverySecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptersubjectdiscoverysecured/AdapterSubjectDiscoverySecured.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterSubjectDiscoverySecured" depends="wsimport-init,wsimport-service-check-AdapterSubjectDiscoverySecured" unless="wsimport-service-AdapterSubjectDiscoverySecured.notRequired">
        <property name="service-wsdl-AdapterSubjectDiscoverySecured" location="${meta.inf}/xml-resources/web-services/AdapterSubjectDiscoverySecured/wsdl/AdapterSubjectDiscoverySecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterSubjectDiscoverySecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterSubjectDiscoverySecured" location="${meta.inf}/wsdl/AdapterSubjectDiscoverySecured"/>
        <property name="sourceLocation-AdapterSubjectDiscoverySecured" location="${meta.inf}/xml-resources/web-services/AdapterSubjectDiscoverySecured/wsdl/"/>
        <copy todir="${targetLocation-AdapterSubjectDiscoverySecured}">
            <fileset dir="${sourceLocation-AdapterSubjectDiscoverySecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterSubjectDiscoverySecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptersubjectdiscoverysecured"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterSubjectDiscoverySecured"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
