<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-AdapterComponentMpi" depends="wsimport-init">
        <condition property="wsimport-service-AdapterComponentMpi.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptercomponentmpi/AdapterComponentMpiService.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterComponentMpi" depends="wsimport-init,wsimport-service-check-AdapterComponentMpi" unless="wsimport-service-AdapterComponentMpi.notRequired">
        <property name="service-wsdl-AdapterComponentMpi" location="${meta.inf}/xml-resources/web-services/AdapterComponentMpi/wsdl/AdapterComponentMpi.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterComponentMpi}" catalog="catalog.xml"/>
        <property name="targetLocation-AdapterComponentMpi" location="${meta.inf}/wsdl/AdapterComponentMpi"/>
        <property name="sourceLocation-AdapterComponentMpi" location="${meta.inf}/xml-resources/web-services/AdapterComponentMpi/wsdl/"/>
        <copy todir="${targetLocation-AdapterComponentMpi}">
            <fileset dir="${sourceLocation-AdapterComponentMpi}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterComponentMpi" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptercomponentmpi"/>
    </target>
    <target name="wsimport-service-check-AdapterComponentMpiSecured" depends="wsimport-init">
        <condition property="wsimport-service-AdapterComponentMpiSecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptercomponentmpi/AdapterComponentMpiSecuredService.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterComponentMpiSecured" depends="wsimport-init,wsimport-service-check-AdapterComponentMpiSecured" unless="wsimport-service-AdapterComponentMpiSecured.notRequired">
        <property name="service-wsdl-AdapterComponentMpiSecured" location="${meta.inf}/xml-resources/web-services/AdapterComponentMpiSecured/wsdl/AdapterComponentSecuredMpi.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterComponentMpiSecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterComponentMpiSecured" location="${meta.inf}/wsdl/AdapterComponentMpiSecured"/>
        <property name="sourceLocation-AdapterComponentMpiSecured" location="${meta.inf}/xml-resources/web-services/AdapterComponentMpiSecured/wsdl/"/>
        <copy todir="${targetLocation-AdapterComponentMpiSecured}">
            <fileset dir="${sourceLocation-AdapterComponentMpiSecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterComponentMpiSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptercomponentmpi"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterComponentMpi, wsimport-service-AdapterComponentMpiSecured"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
