<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-check-AdapterComponentMpi" depends="wsimport-init">
        <condition property="wsimport-client-AdapterComponentMpi.notRequired">
            <available file="${build.generated.dir}/wsimport/client/gov:hhs:fha:nhinc:adaptercomponentmpi/AdapterComponentMpi.java"/>
        </condition>
    </target>
    <target name="wsimport-client-AdapterComponentMpi" depends="wsimport-init,wsimport-client-check-AdapterComponentMpi" unless="wsimport-client-AdapterComponentMpi.notRequired">
        <property name="wsdl-AdapterComponentMpi" location="${meta.inf}/xml-resources/web-service-references/AdapterComponentMpi/wsdl/AdapterComponentMpi.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${wsdl-AdapterComponentMpi}" catalog="catalog.xml"/>
    </target>
    <target name="wsimport-client-clean-AdapterComponentMpi" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/gov:hhs:fha:nhinc:adaptercomponentmpi"/>
    </target>
    <target name="wsimport-client-check-AdapterComponentSecuredMpi" depends="wsimport-init">
        <condition property="wsimport-client-AdapterComponentSecuredMpi.notRequired">
            <available file="${build.generated.dir}/wsimport/client/gov:hhs:fha:nhinc:adaptercomponentmpi/AdapterComponentSecuredMpi.java"/>
        </condition>
    </target>
    <target name="wsimport-client-AdapterComponentSecuredMpi" depends="wsimport-init,wsimport-client-check-AdapterComponentSecuredMpi" unless="wsimport-client-AdapterComponentSecuredMpi.notRequired">
        <property name="wsdl-AdapterComponentSecuredMpi" location="${meta.inf}/xml-resources/web-service-references/AdapterComponentSecuredMpi/wsdl/AdapterComponentSecuredMpi.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${wsdl-AdapterComponentSecuredMpi}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/AdapterComponentSecuredMpi.wsdl" xnocompile="true"/>
    </target>
    <target name="wsimport-client-clean-AdapterComponentSecuredMpi" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/gov:hhs:fha:nhinc:adaptercomponentmpi"/>
    </target>
    <target name="wsimport-service-check-AdapterMpiSecured" depends="wsimport-init">
        <condition property="wsimport-service-AdapterMpiSecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptermpi/AdapterMpiSecuredService.java"/>
        </condition>
    </target>
    <target name="wsimport-service-AdapterMpiSecured" depends="wsimport-init,wsimport-service-check-AdapterMpiSecured" unless="wsimport-service-AdapterMpiSecured.notRequired">
        <property name="service-wsdl-AdapterMpiSecured" location="${meta.inf}/xml-resources/web-services/AdapterMpiSecured/wsdl/AdapterMpiSecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-AdapterMpiSecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-AdapterMpiSecured" location="${meta.inf}/wsdl/AdapterMpiSecured"/>
        <property name="sourceLocation-AdapterMpiSecured" location="${meta.inf}/xml-resources/web-services/AdapterMpiSecured/wsdl/"/>
        <copy todir="${targetLocation-AdapterMpiSecured}">
            <fileset dir="${sourceLocation-AdapterMpiSecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterMpiSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/adaptermpi"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-AdapterComponentMpi, wsimport-client-AdapterComponentSecuredMpi"/>
    <target name="wsimport-client-compile" depends="wsimport-client-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
        <copy todir="${classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterMpiSecured"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
