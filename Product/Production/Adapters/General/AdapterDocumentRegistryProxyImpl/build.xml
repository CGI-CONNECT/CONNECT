  <antcall target="call-target-in-isolation-if-out-of-date" />
</target>
<target name="clean-build" depends="clean,build" />
<target name="clean-test" depends="clean-build,test" />
<target name="build-test" depends="build,test" />

   
    <target depends="init" name="deps-jar" unless="no.deps">
        <property name="project.AdapterDocumentRegistryProxy.Absolute" location="${project.AdapterDocumentRegistryProxy}"/>
        <property name="project.NhincLib.Absolute" location="${project.NhincLib}"/>
        <ant dir="${project.AdapterDocumentRegistryProxy.Absolute}" antfile="${project.AdapterDocumentRegistryProxy.Absolute}/build.xml" inheritall="false" target="jar"/>
        <ant dir="${project.NhincLib.Absolute}" antfile="${project.NhincLib.Absolute}/build.xml" inheritall="false" target="jar"/>
    </target>

    <target depends="init" name="deps-clean" unless="no.deps">
