<project name="ConsumerPreferencesProfileGUI" default="default" basedir=".">
    <description>Builds, tests, and runs the project ConsumerPreferencesProfileGUI.</description>
    <import file="../../../../build.lib.xml" />

    <property name="deploy.type" value="war" />
    <property name="deploy.isDeployable" value="true"/>
    
    <import file="nbproject/build-impl.xml"/>
    
    <target name="build">
      <property name="target-to-call" value="dist" />
      <antcall target="call-target-in-isolation-if-out-of-date" />
    </target>
    <target name="clean-build" depends="clean,dist" />
    <target name="clean-test" depends="clean,dist" />
    <target name="build-test" depends="dist" />
    
    <target name="wsimport-client-identityservices" depends="wsimport-init"/>

    <target depends="init" if="no.dist.ear.dir" name="deps-module-jar" unless="no.deps">
        <property name="project.AdapterAuthenticationLib.Absolute" location="${project.AdapterAuthenticationLib}"/>
        <property name="project.AdapterMpiProxy.Absolute" location="${project.AdapterMpiProxy}"/>
        <property name="project.AdapterMpiProxyImpl.Absolute" location="${project.AdapterMpiProxyImpl}"/>
        <property name="project.AdapterPIPLib.Absolute" location="${project.AdapterPIPLib}"/>
        <property name="project.DocumentRepository.Absolute" location="${project.DocumentRepository}"/>
        <property name="project.MpiLib.Absolute" location="${project.MpiLib}"/>
        <property name="project.NhinHiemProxy.Absolute" location="${project.NhinHiemProxy}"/>

        <ant dir="${project.AdapterAuthenticationLib.Absolute}" antfile="${project.AdapterAuthenticationLib.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.AdapterMpiProxy.Absolute}" antfile="${project.AdapterMpiProxy.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.AdapterMpiProxyImpl.Absolute}" antfile="${project.AdapterMpiProxyImpl.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.AdapterPIPLib.Absolute}" antfile="${project.AdapterPIPLib.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.DocumentRepository.Absolute}" antfile="${project.DocumentRepository.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.MpiLib.Absolute}" antfile="${project.MpiLib.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.NhinHiemProxy.Absolute}" antfile="${project.NhinHiemProxy.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
    </target>
    <target depends="init" if="dist.ear.dir" name="deps-ear-jar" unless="no.deps">
        <property name="project.AdapterAuthenticationLib.Absolute" location="${project.AdapterAuthenticationLib}"/>
        <property name="project.AdapterMpiProxy.Absolute" location="${project.AdapterMpiProxy}"/>
        <property name="project.AdapterMpiProxyImpl.Absolute" location="${project.AdapterMpiProxyImpl}"/>
        <property name="project.AdapterPIPLib.Absolute" location="${project.AdapterPIPLib}"/>
        <property name="project.DocumentRepository.Absolute" location="${project.DocumentRepository}"/>
        <property name="project.MpiLib.Absolute" location="${project.MpiLib}"/>
        <property name="project.NhinHiemProxy.Absolute" location="${project.NhinHiemProxy}"/>
        <property name="project.NhincDataTransformsLib.Absolute" location="${project.NhincDataTransformsLib}"/>
        <property name="project.NhincLib.Absolute" location="${project.NhincLib}"/>

        <ant dir="${project.AdapterAuthenticationLib.Absolute}" antfile="${project.AdapterAuthenticationLib.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.AdapterMpiProxy.Absolute}" antfile="${project.AdapterMpiProxy.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.AdapterMpiProxyImpl.Absolute}" antfile="${project.AdapterMpiProxyImpl.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.AdapterPIPLib.Absolute}" antfile="${project.AdapterPIPLib.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.DocumentRepository.Absolute}" antfile="${project.DocumentRepository.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.MpiLib.Absolute}" antfile="${project.MpiLib.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
        <ant dir="${project.NhinHiemProxy.Absolute}" antfile="${project.NhinHiemProxy.Absolute}/build.xml" inheritall="false" target="jar">
            <property name="deploy.on.save" value="false"/>
        </ant>
    </target>
    
    <target depends="init" if="no.dist.ear.dir" name="deps-clean" unless="no.deps">
        <property name="project.AdapterAuthenticationLib.Absolute" location="${project.AdapterAuthenticationLib}"/>
        <property name="project.AdapterMpiProxy.Absolute" location="${project.AdapterMpiProxy}"/>
        <property name="project.AdapterMpiProxyImpl.Absolute" location="${project.AdapterMpiProxyImpl}"/>
        <property name="project.AdapterPIPLib.Absolute" location="${project.AdapterPIPLib}"/>
        <property name="project.DocumentRepository.Absolute" location="${project.DocumentRepository}"/>
        <property name="project.MpiLib.Absolute" location="${project.MpiLib}"/>
        <property name="project.NhinHiemProxy.Absolute" location="${project.NhinHiemProxy}"/>

        <ant dir="${project.AdapterAuthenticationLib.Absolute}" antfile="${project.AdapterAuthenticationLib.Absolute}/build.xml" inheritall="false" target="clean"/>
        <ant dir="${project.AdapterMpiProxy.Absolute}" antfile="${project.AdapterMpiProxy.Absolute}/build.xml" inheritall="false" target="clean"/>
        <ant dir="${project.AdapterMpiProxyImpl.Absolute}" antfile="${project.AdapterMpiProxyImpl.Absolute}/build.xml" inheritall="false" target="clean"/>
        <ant dir="${project.AdapterPIPLib.Absolute}" antfile="${project.AdapterPIPLib.Absolute}/build.xml" inheritall="false" target="clean"/>
        <ant dir="${project.DocumentRepository.Absolute}" antfile="${project.DocumentRepository.Absolute}/build.xml" inheritall="false" target="clean"/>
        <ant dir="${project.MpiLib.Absolute}" antfile="${project.MpiLib.Absolute}/build.xml" inheritall="false" target="clean"/>
        <ant dir="${project.NhinHiemProxy.Absolute}" antfile="${project.NhinHiemProxy.Absolute}/build.xml" inheritall="false" target="clean"/>
    </target>

    <target depends="init" if="netbeans.home" name="undeploy-clean" unless="from.commandline">
        <nbundeploy failOnError="false" startServer="false"/>
    </target>
</project>
