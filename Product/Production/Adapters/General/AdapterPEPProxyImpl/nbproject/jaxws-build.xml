<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-AdapterPEP" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/AdapterPEP"/>
        <property name="wsdl-AdapterPEP" location="xml-resources/web-service-references/AdapterPEP/wsdl/AdapterPEP.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/AdapterPEP" destdir="${build.generated.dir}/jax-wsCache/AdapterPEP" wsdl="${wsdl-AdapterPEP}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/AdapterPEP.wsdl" xnocompile="true">
            <depends file="${wsdl-AdapterPEP}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/AdapterPEP"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/AdapterPEP">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-AdapterPEP" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/AdapterPEP"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov:hhs:fha:nhinc:adapterpep"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-AdapterPEP"/>
</project>
