<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-AdapterAuthenticationService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/AdapterAuthenticationService"/>
        <property name="service-wsdl-AdapterAuthenticationService" location="${meta.inf}/xml-resources/web-services/AdapterAuthenticationService/wsdl/AdapterAuthentication.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/AdapterAuthenticationService" destdir="${build.generated.dir}/jax-wsCache/service/AdapterAuthenticationService" wsdl="${service-wsdl-AdapterAuthenticationService}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-AdapterAuthenticationService}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/AdapterAuthenticationService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/AdapterAuthenticationService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-AdapterAuthenticationService" location="${meta.inf}/wsdl/AdapterAuthenticationService"/>
        <property name="sourceLocation-AdapterAuthenticationService" location="${meta.inf}/xml-resources/web-services/AdapterAuthenticationService/wsdl/"/>
        <copy todir="${targetLocation-AdapterAuthenticationService}">
            <fileset dir="${sourceLocation-AdapterAuthenticationService}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AdapterAuthenticationService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/AdapterAuthenticationService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/adapterauthentication"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AdapterAuthenticationService"/>
</project>
