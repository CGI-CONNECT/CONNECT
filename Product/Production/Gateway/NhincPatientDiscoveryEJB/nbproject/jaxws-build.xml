<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-NhinPatientDiscovery" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/NhinPatientDiscovery"/>
        <property name="service-wsdl-NhinPatientDiscovery" location="${meta.inf}/xml-resources/web-services/NhinPatientDiscovery/wsdl/NhinPatientDiscovery.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/NhinPatientDiscovery" destdir="${build.generated.dir}/jax-wsCache/service/NhinPatientDiscovery" wsdl="${service-wsdl-NhinPatientDiscovery}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-NhinPatientDiscovery}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/NhinPatientDiscovery"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/NhinPatientDiscovery">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-NhinPatientDiscovery" location="${meta.inf}/wsdl/NhinPatientDiscovery"/>
        <property name="sourceLocation-NhinPatientDiscovery" location="${meta.inf}/xml-resources/web-services/NhinPatientDiscovery/wsdl/"/>
        <copy todir="${targetLocation-NhinPatientDiscovery}">
            <fileset dir="${sourceLocation-NhinPatientDiscovery}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhinPatientDiscovery" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/NhinPatientDiscovery"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/ihe/iti/xcpd/_2009"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-NhinPatientDiscovery"/>
</project>
