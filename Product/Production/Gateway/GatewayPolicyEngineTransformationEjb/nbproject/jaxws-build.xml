<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-PolicyEngineTransformationService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/PolicyEngineTransformationService"/>
        <property name="service-wsdl-PolicyEngineTransformationService" location="${meta.inf}/xml-resources/web-services/PolicyEngineTransformationService/wsdl/NhincComponentInternalPolicyEngineTransform.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/PolicyEngineTransformationService" destdir="${build.generated.dir}/jax-wsCache/service/PolicyEngineTransformationService" wsdl="${service-wsdl-PolicyEngineTransformationService}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/PolicyEngineTransformationService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-PolicyEngineTransformationService" location="${meta.inf}/wsdl/PolicyEngineTransformationService"/>
        <property name="sourceLocation-PolicyEngineTransformationService" location="${meta.inf}/xml-resources/web-services/PolicyEngineTransformationService/wsdl/"/>
        <copy todir="${targetLocation-PolicyEngineTransformationService}">
            <fileset dir="${sourceLocation-PolicyEngineTransformationService}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-PolicyEngineTransformationService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/PolicyEngineTransformationService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhincinternalcomponentpolicyenginetransform"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-PolicyEngineTransformationService"/>
</project>
