<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-NhinSubjectDiscovery" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/NhinSubjectDiscovery"/>
        <property name="wsdl-NhinSubjectDiscovery" location="xml-resources/web-service-references/NhinSubjectDiscovery/wsdl/NhinSubjectDiscovery.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/NhinSubjectDiscovery" destdir="${build.generated.dir}/jax-wsCache/NhinSubjectDiscovery" wsdl="${wsdl-NhinSubjectDiscovery}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/NhinSubjectDiscovery.wsdl" xnocompile="true">
            <depends file="${wsdl-NhinSubjectDiscovery}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/NhinSubjectDiscovery"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/NhinSubjectDiscovery">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-NhinSubjectDiscovery" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/NhinSubjectDiscovery"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/ihe:iti:pixv3:2007"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-NhinSubjectDiscovery"/>
</project>
