<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:webproject2="http://www.netbeans.org/ns/web-project/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${java.home}/../lib/tools.jar:${j2ee.platform.wsimport.classpath}:${javac.classpath}"/>
        </taskdef>
        <condition property="conf-dir" value="${conf.dir}/" else="">
            <isset property="conf.dir"/>
        </condition>
    </target>
    <target name="wsimport-service-NhincProxyDocQuerySecured" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocQuerySecured"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocQuerySecured" destdir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocQuerySecured" wsdl="${basedir}/${conf-dir}xml-resources/web-services/NhincProxyDocQuerySecured/wsdl/NhincProxyDocQuerySecured.wsdl" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-services/NhincProxyDocQuerySecured/wsdl/NhincProxyDocQuerySecured.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocQuerySecured"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocQuerySecured">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <copy todir="${build.web.dir}/WEB-INF/wsdl/NhincProxyDocQuerySecured">
            <fileset dir="${basedir}/${conf-dir}xml-resources/web-services/NhincProxyDocQuerySecured/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincProxyDocQuerySecured" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhincproxydocquerysecured"/>
        <delete dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocQuerySecured"/>
    </target>
    <target name="wsimport-service-NhincProxyDocRetrieveSecured" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocRetrieveSecured"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocRetrieveSecured" destdir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocRetrieveSecured" wsdl="${basedir}/${conf-dir}xml-resources/web-services/NhincProxyDocRetrieveSecured/wsdl/NhincProxyDocRetrieveSecured.wsdl" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-services/NhincProxyDocRetrieveSecured/wsdl/NhincProxyDocRetrieveSecured.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocRetrieveSecured"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocRetrieveSecured">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <copy todir="${build.web.dir}/WEB-INF/wsdl/NhincProxyDocRetrieveSecured">
            <fileset dir="${basedir}/${conf-dir}xml-resources/web-services/NhincProxyDocRetrieveSecured/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincProxyDocRetrieveSecured" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhincproxydocretrievesecured"/>
        <delete dir="${build.generated.dir}/jax-wsCache/service/NhincProxyDocRetrieveSecured"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-NhincProxyDocQuerySecured, wsimport-service-NhincProxyDocRetrieveSecured"/>
</project>
