<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-PatientCorrelationServiceSecured" depends="wsimport-init">
        <condition property="wsimport-service-PatientCorrelationServiceSecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhinccomponentpatientcorrelation/PatientCorrelationServiceSecured.java"/>
        </condition>
    </target>
    <target name="wsimport-service-PatientCorrelationServiceSecured" depends="wsimport-init,wsimport-service-check-PatientCorrelationServiceSecured" unless="wsimport-service-PatientCorrelationServiceSecured.notRequired">
        <property name="service-wsdl-PatientCorrelationServiceSecured" location="${meta.inf}/xml-resources/web-services/PatientCorrelationServiceSecured/wsdl/NhincComponentPatientCorrelationSecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-PatientCorrelationServiceSecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-PatientCorrelationServiceSecured" location="${meta.inf}/wsdl/PatientCorrelationServiceSecured"/>
        <property name="sourceLocation-PatientCorrelationServiceSecured" location="${meta.inf}/xml-resources/web-services/PatientCorrelationServiceSecured/wsdl/"/>
        <copy todir="${targetLocation-PatientCorrelationServiceSecured}">
            <fileset dir="${sourceLocation-PatientCorrelationServiceSecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-PatientCorrelationServiceSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhinccomponentpatientcorrelation"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-PatientCorrelationServiceSecured"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
