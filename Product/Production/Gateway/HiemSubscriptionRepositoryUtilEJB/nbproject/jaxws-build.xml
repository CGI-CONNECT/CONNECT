<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-HiemSubscriptionRepositoryUtilService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/HiemSubscriptionRepositoryUtilService"/>
        <property name="service-wsdl-HiemSubscriptionRepositoryUtilService" location="${meta.inf}/xml-resources/web-services/HiemSubscriptionRepositoryUtilService/wsdl/HiemSubscriptionRepositoryUtil.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/HiemSubscriptionRepositoryUtilService" destdir="${build.generated.dir}/jax-wsCache/service/HiemSubscriptionRepositoryUtilService" wsdl="${service-wsdl-HiemSubscriptionRepositoryUtilService}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-HiemSubscriptionRepositoryUtilService}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/HiemSubscriptionRepositoryUtilService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/HiemSubscriptionRepositoryUtilService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-HiemSubscriptionRepositoryUtilService" location="${meta.inf}/wsdl/HiemSubscriptionRepositoryUtilService"/>
        <property name="sourceLocation-HiemSubscriptionRepositoryUtilService" location="${meta.inf}/xml-resources/web-services/HiemSubscriptionRepositoryUtilService/wsdl/"/>
        <copy todir="${targetLocation-HiemSubscriptionRepositoryUtilService}">
            <fileset dir="${sourceLocation-HiemSubscriptionRepositoryUtilService}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-HiemSubscriptionRepositoryUtilService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/HiemSubscriptionRepositoryUtilService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/hiemsubscriptionrepositoryutil"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-HiemSubscriptionRepositoryUtilService"/>
</project>
