<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-NhincSubscription" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/NhincSubscription"/>
        <property name="wsdl-NhincSubscription" location="${meta.inf}/xml-resources/web-service-references/NhincSubscription/wsdl/NhincSubscription.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/NhincSubscription" destdir="${build.generated.dir}/jax-wsCache/NhincSubscription" wsdl="${wsdl-NhincSubscription}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/NhincSubscription">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-NhincSubscription" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/NhincSubscription"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhincsubscription"/>
    </target>
    <target name="wsimport-service-HiemUnsubscribe" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/HiemUnsubscribe"/>
        <property name="service-wsdl-HiemUnsubscribe" location="${meta.inf}/xml-resources/web-services/HiemUnsubscribe/wsdl/NhinSubscription.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/HiemUnsubscribe" destdir="${build.generated.dir}/jax-wsCache/service/HiemUnsubscribe" wsdl="${service-wsdl-HiemUnsubscribe}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/HiemUnsubscribe">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-HiemUnsubscribe" location="${meta.inf}/wsdl/HiemUnsubscribe"/>
        <property name="sourceLocation-HiemUnsubscribe" location="${meta.inf}/xml-resources/web-services/HiemUnsubscribe/wsdl/"/>
        <copy todir="${targetLocation-HiemUnsubscribe}">
            <fileset dir="${sourceLocation-HiemUnsubscribe}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-HiemUnsubscribe" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/HiemUnsubscribe"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/org/oasis_open/docs/wsn/bw_2"/>
    </target>
    <target name="wsimport-service-HiemNotify" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/HiemNotify"/>
        <property name="service-wsdl-HiemNotify" location="${meta.inf}/xml-resources/web-services/HiemNotify/wsdl/NhinSubscription.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/HiemNotify" destdir="${build.generated.dir}/jax-wsCache/service/HiemNotify" wsdl="${service-wsdl-HiemNotify}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/HiemNotify">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-HiemNotify" location="${meta.inf}/wsdl/HiemNotify"/>
        <property name="sourceLocation-HiemNotify" location="${meta.inf}/xml-resources/web-services/HiemNotify/wsdl/"/>
        <copy todir="${targetLocation-HiemNotify}">
            <fileset dir="${sourceLocation-HiemNotify}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-HiemNotify" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/HiemNotify"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/org/oasis_open/docs/wsn/bw_2"/>
    </target>
    <target name="wsimport-service-HiemSubscription" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/HiemSubscription"/>
        <property name="service-wsdl-HiemSubscription" location="${meta.inf}/xml-resources/web-services/HiemSubscription/wsdl/NhinSubscription.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/HiemSubscription" destdir="${build.generated.dir}/jax-wsCache/service/HiemSubscription" wsdl="${service-wsdl-HiemSubscription}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-HiemSubscription}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/HiemSubscription"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/HiemSubscription">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-HiemSubscription" location="${meta.inf}/wsdl/HiemSubscription"/>
        <property name="sourceLocation-HiemSubscription" location="${meta.inf}/xml-resources/web-services/HiemSubscription/wsdl/"/>
        <copy todir="${targetLocation-HiemSubscription}">
            <fileset dir="${sourceLocation-HiemSubscription}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-HiemSubscription" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/HiemSubscription"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/org/oasis_open/docs/wsn/bw_2"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-NhincSubscription"/>
    <target name="wsimport-service-generate" depends="wsimport-service-HiemUnsubscribe, wsimport-service-HiemNotify, wsimport-service-HiemSubscription"/>
</project>
