<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-NhincProxyDocRetrieveSecured" depends="wsimport-init">
        <condition property="wsimport-service-NhincProxyDocRetrieveSecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxydocretrievesecured/NhincProxyDocRetrieveSecured.java"/>
        </condition>
    </target>
    <target name="wsimport-service-NhincProxyDocRetrieveSecured" depends="wsimport-init,wsimport-service-check-NhincProxyDocRetrieveSecured" unless="wsimport-service-NhincProxyDocRetrieveSecured.notRequired">
        <property name="service-wsdl-NhincProxyDocRetrieveSecured" location="${meta.inf}/xml-resources/web-services/NhincProxyDocRetrieveSecured/wsdl/NhincProxyDocRetrieveSecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-NhincProxyDocRetrieveSecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-NhincProxyDocRetrieveSecured" location="${meta.inf}/wsdl/NhincProxyDocRetrieveSecured"/>
        <property name="sourceLocation-NhincProxyDocRetrieveSecured" location="${meta.inf}/xml-resources/web-services/NhincProxyDocRetrieveSecured/wsdl/"/>
        <copy todir="${targetLocation-NhincProxyDocRetrieveSecured}">
            <fileset dir="${sourceLocation-NhincProxyDocRetrieveSecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincProxyDocRetrieveSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxydocretrievesecured"/>
    </target>
    <target name="wsimport-service-check-EntityDocRetrieveSecured" depends="wsimport-init">
        <condition property="wsimport-service-EntityDocRetrieveSecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entitydocretrievesecured/EntityDocRetrieveSecured.java"/>
        </condition>
    </target>
    <target name="wsimport-service-EntityDocRetrieveSecured" depends="wsimport-init,wsimport-service-check-EntityDocRetrieveSecured" unless="wsimport-service-EntityDocRetrieveSecured.notRequired">
        <property name="service-wsdl-EntityDocRetrieveSecured" location="${meta.inf}/xml-resources/web-services/EntityDocRetrieveSecured/wsdl/EntityDocRetrieveSecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-EntityDocRetrieveSecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-EntityDocRetrieveSecured" location="${meta.inf}/wsdl/EntityDocRetrieveSecured"/>
        <property name="sourceLocation-EntityDocRetrieveSecured" location="${meta.inf}/xml-resources/web-services/EntityDocRetrieveSecured/wsdl/"/>
        <copy todir="${targetLocation-EntityDocRetrieveSecured}">
            <fileset dir="${sourceLocation-EntityDocRetrieveSecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-EntityDocRetrieveSecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entitydocretrievesecured"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-NhincProxyDocRetrieveSecured, wsimport-service-EntityDocRetrieveSecured"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
