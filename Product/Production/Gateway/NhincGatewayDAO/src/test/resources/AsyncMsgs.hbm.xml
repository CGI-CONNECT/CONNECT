<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Document   : asyncmsgs.hbm.xml
    Created on : April 12, 2010
    Author     : jhoppesch
    Description:
        Purpose of the document follows.
-->
<hibernate-mapping>
	<class name="gov.hhs.fha.nhinc.asyncmsgs.model.AsyncMsgRecord" table="asyncmsgrepo">
		<id name="messageId" column="MessageId" length="100"  type="string"/>
		<property column="CreationTime" name="creationTime" not-null="true" type="timestamp"/>
		<property column="ServiceName" length="100" name="serviceName" not-null="false" type="string"/>
		<property column="MsgData" name="msgData" not-null="false" type="binary"/>
	</class>
</hibernate-mapping>
