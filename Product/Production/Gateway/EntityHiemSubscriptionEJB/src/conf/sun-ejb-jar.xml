<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 EJB 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_0-0.dtd">
<sun-ejb-jar>
    <enterprise-beans>
    <ejb>
            <ejb-name>EntitySubscribeService</ejb-name>
            <webservice-endpoint>
                <port-component-name>EntitySubscribeService</port-component-name>
                <endpoint-address-uri>/NhinConnect/EntityNotificationProducer</endpoint-address-uri>
            </webservice-endpoint>   
        </ejb>
        <ejb>
            <ejb-name>EntityNotifyService</ejb-name>
            <webservice-endpoint>
                <port-component-name>EntityNotifyService</port-component-name>
                <endpoint-address-uri>/NhinConnect/EntityNotificationConsumer</endpoint-address-uri>
            </webservice-endpoint>   
        </ejb>
        <ejb>
            <ejb-name>ProxyHiemSubscribe</ejb-name>
            <webservice-endpoint>
                <port-component-name>ProxyHiemSubscribe</port-component-name>
                <endpoint-address-uri>/NhinConnect/NhincProxyNotificationProducer</endpoint-address-uri>
            </webservice-endpoint>   
        </ejb>
        <ejb>
            <ejb-name>ProxyHiemNotify</ejb-name>
            <webservice-endpoint>
                <port-component-name>ProxyHiemNotify</port-component-name>
                <endpoint-address-uri>/NhinConnect/NhincProxyNotificationConsumer</endpoint-address-uri>
            </webservice-endpoint>   
        </ejb>
    </enterprise-beans>
</sun-ejb-jar>
