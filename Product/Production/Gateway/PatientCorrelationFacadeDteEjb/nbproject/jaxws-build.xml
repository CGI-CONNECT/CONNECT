<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-PatientCorrelationFacadeDteService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService"/>
        <property name="service-wsdl-PatientCorrelationFacadeDteService" location="${meta.inf}/xml-resources/web-services/PatientCorrelationFacadeDteService/wsdl/NhincComponentPatientCorrelationFacadeDte.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService" destdir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService" wsdl="${service-wsdl-PatientCorrelationFacadeDteService}" catalog="catalog.xml"/>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-PatientCorrelationFacadeDteService" location="${meta.inf}/wsdl/PatientCorrelationFacadeDteService"/>
        <property name="sourceLocation-PatientCorrelationFacadeDteService" location="${meta.inf}/xml-resources/web-services/PatientCorrelationFacadeDteService/wsdl/"/>
        <copy todir="${targetLocation-PatientCorrelationFacadeDteService}">
            <fileset dir="${sourceLocation-PatientCorrelationFacadeDteService}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-PatientCorrelationFacadeDteService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/componentpatientcorrelationfacadedte"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-PatientCorrelationFacadeDteService"/>
</project>
