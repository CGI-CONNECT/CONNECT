        <servlet-name>NhinXDR_g1</servlet-name>
        <url-pattern>/DocumentRepositoryXDR_Service</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NhinXDRRequest_g1</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docsubmission_g1.nhin.deferred.request.NhinXDRRequest_g1</servlet-class>
