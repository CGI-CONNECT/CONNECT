<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
   
    <servlet>
        <servlet-name>HiemNotify</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.hiem.nhin.notify.HiemNotify</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemNotify</servlet-name>
        <url-pattern>/NhinService/NotificationConsumerService/HiemNotify</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HiemSubscription</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.hiem.nhin.subscription.HiemSubscription</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemSubscription</servlet-name>
        <url-pattern>/NhinService/NotificationProducerService/HiemSubscription</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HiemUnsubscribe</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.hiem.nhin.unsubscribe.HiemUnsubscribe</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemUnsubscribe</servlet-name>
        <url-pattern>/NhinService/SubscriptionManagerService/HiemUnsubscribe</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SubjectDiscovery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.subjectdiscovery.nhin.SubjectDiscovery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubjectDiscovery</servlet-name>
        <url-pattern>/NhinService/NhinSubjectDiscovery</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinXDR</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docsubmission.nhin.NhinXDR</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDR</servlet-name>
        <url-pattern>/DocumentRepositoryXDR_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinXDRRequest</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docsubmission.nhin.deferred.request.NhinXDRRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDRRequest</servlet-name>
        <url-pattern>/NhinService/XDRRequest_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinXDRResponse</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docsubmission.nhin.deferred.response.NhinXDRResponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDRResponse</servlet-name>
        <url-pattern>/NhinService/XDRResponse_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AuditQuery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.auditquery.nhin.AuditQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuditQuery</servlet-name>
        <url-pattern>/NhinService/findAuditEvents/AuditQuery</url-pattern>
    </servlet-mapping>
    <security-constraint>
        <web-resource-collection>
            <web-resource-name>HiemNotify</web-resource-name>
            <url-pattern>/NhinService/NotificationConsumerService/HiemNotify</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemSubscription</web-resource-name>
            <url-pattern>/NhinService/NotificationProducerService/HiemSubscription</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemUnsubscribe</web-resource-name>
            <url-pattern>/NhinService/SubscriptionManagerService/HiemUnsubscribe</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>SubjectDiscovery</web-resource-name>
            <url-pattern>/NhinService/NhinSubjectDiscovery</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDR</web-resource-name>
            <url-pattern>/DocumentRepositoryXDR_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDRRequest</web-resource-name>
            <url-pattern>/NhinService/XDRRequest_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDRResponse</web-resource-name>
            <url-pattern>/NhinService/XDRResponse_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>AuditQuery</web-resource-name>
            <url-pattern>/NhinService/findAuditEvents/AuditQuery</url-pattern>
        </web-resource-collection>
      
        <user-data-constraint>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
    </security-constraint>
    <login-config>
        <auth-method>CLIENT-CERT</auth-method>
        <realm-name>certificate</realm-name>
    </login-config>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
