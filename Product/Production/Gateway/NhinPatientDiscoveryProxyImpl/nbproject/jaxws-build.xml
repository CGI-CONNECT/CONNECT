<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-NhinPatientDiscovery" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/NhinPatientDiscovery"/>
        <property name="wsdl-NhinPatientDiscovery" location="xml-resources/web-service-references/NhinPatientDiscovery/wsdl/NhinPatientDiscovery.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/NhinPatientDiscovery" destdir="${build.generated.dir}/jax-wsCache/NhinPatientDiscovery" wsdl="${wsdl-NhinPatientDiscovery}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/NhinPatientDiscovery.wsdl" xnocompile="true">
            <depends file="${wsdl-NhinPatientDiscovery}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/NhinPatientDiscovery"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/NhinPatientDiscovery">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-NhinPatientDiscovery" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/NhinPatientDiscovery"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/ihe/iti/xcpd/_2009"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-NhinPatientDiscovery"/>
</project>
