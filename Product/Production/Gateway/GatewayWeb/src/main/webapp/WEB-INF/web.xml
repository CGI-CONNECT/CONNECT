        <servlet-class>gov.hhs.fha.nhinc.subscription.repository.util.HiemSubscriptionRepositoryUtilService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemSubscriptionRepositoryUtilService</servlet-name>
        <url-pattern>/GatewayInternalService/HiemSubscriptionRepositoryUtil</url-pattern>
    </servlet-mapping>

