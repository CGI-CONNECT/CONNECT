<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-EntityAuditLogQuery" depends="wsimport-init">
        <condition property="wsimport-service-EntityAuditLogQuery.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entityauditlogquerysaml/EntityAuditLogQuerySamlService.java"/>
        </condition>
    </target>
    <target name="wsimport-service-EntityAuditLogQuery" depends="wsimport-init,wsimport-service-check-EntityAuditLogQuery" unless="wsimport-service-EntityAuditLogQuery.notRequired">
        <property name="service-wsdl-EntityAuditLogQuery" location="${meta.inf}/xml-resources/web-services/EntityAuditLogQuery/wsdl/EntityAuditLogQuerySecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-EntityAuditLogQuery}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-EntityAuditLogQuery" location="${meta.inf}/wsdl/EntityAuditLogQuery"/>
        <property name="sourceLocation-EntityAuditLogQuery" location="${meta.inf}/xml-resources/web-services/EntityAuditLogQuery/wsdl/"/>
        <copy todir="${targetLocation-EntityAuditLogQuery}">
            <fileset dir="${sourceLocation-EntityAuditLogQuery}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-EntityAuditLogQuery" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entityauditlogquerysaml"/>
    </target>
    <target name="wsimport-service-check-NhincAuditQueryProxy" depends="wsimport-init">
        <condition property="wsimport-service-NhincAuditQueryProxy.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxyauditlogquerysecured/NhincProxyAuditLogQuerySecured.java"/>
        </condition>
    </target>
    <target name="wsimport-service-NhincAuditQueryProxy" depends="wsimport-init,wsimport-service-check-NhincAuditQueryProxy" unless="wsimport-service-NhincAuditQueryProxy.notRequired">
        <property name="service-wsdl-NhincAuditQueryProxy" location="${meta.inf}/xml-resources/web-services/NhincAuditQueryProxy/wsdl/NhincProxyAuditLogQuerySecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-NhincAuditQueryProxy}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-NhincAuditQueryProxy" location="${meta.inf}/wsdl/NhincAuditQueryProxy"/>
        <property name="sourceLocation-NhincAuditQueryProxy" location="${meta.inf}/xml-resources/web-services/NhincAuditQueryProxy/wsdl/"/>
        <copy todir="${targetLocation-NhincAuditQueryProxy}">
            <fileset dir="${sourceLocation-NhincAuditQueryProxy}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincAuditQueryProxy" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxyauditlogquerysecured"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-EntityAuditLogQuery, wsimport-service-NhincAuditQueryProxy"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
