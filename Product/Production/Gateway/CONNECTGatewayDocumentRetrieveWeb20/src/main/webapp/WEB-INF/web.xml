	</servlet>
	<servlet-mapping>
		<servlet-name>PassthruDocRetrieveDeferredRequestSecured</servlet-name>
		<url-pattern>/EntityService/NhincProxyDocRetrieveDeferredRequestSecured</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PassthruDocRetrieveDeferredResponseUnsecured</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.passthru.deferred.response.PassthruDocRetrieveDeferredResponseUnsecured</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PassthruDocRetrieveDeferredResponseUnsecured</servlet-name>
		<url-pattern>/EntityService/NhincProxyDocRetrieveDeferredResponse</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PassthruDocRetrieveDeferredResponseSecured</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.passthru.deferred.response.PassthruDocRetrieveDeferredResponseSecured</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PassthruDocRetrieveDeferredResponseSecured</servlet-name>
		<url-pattern>/EntityService/NhincProxyDocRetrieveDeferredResponseSecured</url-pattern>
