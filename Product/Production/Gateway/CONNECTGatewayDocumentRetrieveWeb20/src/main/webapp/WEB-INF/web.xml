	</servlet>
	<servlet-mapping>
		<servlet-name>EntityDocRetrieveSecured</servlet-name>
		<url-pattern>/EntityService/EntityDocRetrieveSecured</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EntityDocRetrieve</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.entity.EntityDocRetrieve</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EntityDocRetrieve</servlet-name>
		<url-pattern>/EntityService/EntityDocRetrieve</url-pattern>
	</servlet-mapping>

	<!-- message proxy -->

	<servlet>
		<servlet-name>NhincProxyDocRetrieveUnsecured</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.passthru.NhincProxyDocRetrieveUnsecured</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NhincProxyDocRetrieveUnsecured</servlet-name>
		<url-pattern>/NhincProxyDocRetrieveUnsecured</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>NhincProxyDocRetrieveSecured</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.passthru.NhincProxyDocRetrieveSecured</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NhincProxyDocRetrieveSecured</servlet-name>
		<url-pattern>/EntityService/NhincProxyDocRetrieveSecured</url-pattern>
	</servlet-mapping>


<servlet>
        <servlet-name>DocRetrieve</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docretrieve.nhin.DocRetrieve</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DocRetrieve</servlet-name>
        <url-pattern>/NhinService/RespondingGateway_Retrieve_Service/DocRetrieve</url-pattern>
    </servlet-mapping>


	<!-- BEGIN: Document Retrieve Deferred Services -->
	<servlet>
		<servlet-name>EntityDocRetrieveDeferredReq</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.entity.deferred.request.EntityDocRetrieveDeferredReq</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EntityDocRetrieveDeferredReq</servlet-name>
		<url-pattern>/EntityService/EntityDocRetrieveDeferredRequest</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EntityDocRetrieveDeferredReqSecured</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.entity.deferred.request.EntityDocRetrieveDeferredReqSecured</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EntityDocRetrieveDeferredReqSecured</servlet-name>
		<url-pattern>/EntityService/EntityDocRetrieveDeferredRequestSecured</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EntityDocRetrieveDeferredResp</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.entity.deferred.response.EntityDocRetrieveDeferredResp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EntityDocRetrieveDeferredResp</servlet-name>
