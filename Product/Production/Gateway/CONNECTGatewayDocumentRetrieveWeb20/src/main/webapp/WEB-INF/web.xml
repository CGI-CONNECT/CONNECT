		<url-pattern>/EntityService/EntityDocRetrieveDeferredResponseSecured</url-pattern>
	</servlet-mapping>

	<!-- Begin Nhinc proxy Document retrieve deferred services -->
	<servlet>
		<servlet-name>PassthruDocRetrieveDeferredRequestUnsecured</servlet-name>
		<servlet-class>gov.hhs.fha.nhinc.docretrieve.passthru.deferred.request.PassthruDocRetrieveDeferredRequestUnsecured</servlet-class>
