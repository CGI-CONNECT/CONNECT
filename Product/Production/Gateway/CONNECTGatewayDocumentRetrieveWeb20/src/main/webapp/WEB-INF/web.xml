	   <!-- BEGIN: Document Retrieve Deferred Services -->
    <servlet>
        <servlet-name>NhinDocRetrieveDeferredRequest</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docretrieve.nhin.deferred.request.NhinDocRetrieveDeferredRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinDocRetrieveDeferredRequest</servlet-name>
        <url-pattern>/NhinService/RespondingGatewayDeferredRequest_Retrieve_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinDocRetrieveDeferredResponse</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docretrieve.nhin.deferred.response.NhinDocRetrieveDeferredResponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinDocRetrieveDeferredResponse</servlet-name>
        <url-pattern>/NhinService/RespondingGatewayDeferredResponse_Retrieve_Service</url-pattern>
    </servlet-mapping>

	<security-constraint>
	
		<web-resource-collection>
            <web-resource-name>DocRetrieve</web-resource-name>
            <url-pattern>/NhinService/RespondingGateway_Retrieve_Service/DocRetrieve</url-pattern>
        </web-resource-collection>
	
		<!-- BEGIN: Document Retrieve Services -->
		<web-resource-collection>
			<web-resource-name>EntityDocRetrieveSecured</web-resource-name>
			<url-pattern>/EntityService/EntityDocRetrieveSecured</url-pattern>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>EntityDocRetrieveDeferredRespSecured</web-resource-name>
			<url-pattern>/EntityService/EntityDocRetrieveDeferredResponseSecured</url-pattern>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>EntityDocRetrieveDeferredReqSecured</web-resource-name>
			<url-pattern>/EntityService/EntityDocRetrieveDeferredRequestSecured</url-pattern>
		</web-resource-collection>
		<!-- END: Document Retrieve Services -->

		<web-resource-collection>
			<web-resource-name>NhincProxyDocRetrieveSecured</web-resource-name>
			<url-pattern>/EntityService/NhincProxyDocRetrieveSecured</url-pattern>
		</web-resource-collection>

		<!-- Begin Nhinc proxy Document retrieve deferred services -->
		<web-resource-collection>
			<web-resource-name>PassthruDocRetrieveDeferredRequestSecured</web-resource-name>
			<url-pattern>/EntityService/NhincProxyDocRetrieveDeferredRequestSecured</url-pattern>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>PassthruDocRetrieveDeferredResponseSecured</web-resource-name>
			<url-pattern>/EntityService/NhincProxyDocRetrieveDeferredResponseSecured</url-pattern>
		</web-resource-collection>
		<!-- End Nhinc proxy Document retrieve deferred services -->


		  <!-- BEGIN Document Retrieve Deferred Service -->
        <web-resource-collection>
            <web-resource-name>NhinDocRetrieveDeferredRequest</web-resource-name>
            <url-pattern>/NhinService/RespondingGatewayDeferredRequest_Retrieve_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinDocRetrieveDeferredResponse</web-resource-name>
            <url-pattern>/NhinService/RespondingGatewayDeferredResponse_Retrieve_Service</url-pattern>
        </web-resource-collection>
        <!-- END Document Retrieve Deferred Service -->

		<user-data-constraint>
			<transport-guarantee>CONFIDENTIAL</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
	<login-config>
		<auth-method>CLIENT-CERT</auth-method>
		<realm-name>certificate</realm-name>
	</login-config>

	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
