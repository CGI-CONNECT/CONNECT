<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-EntityDocQuerySecured" depends="wsimport-init">
        <condition property="wsimport-service-EntityDocQuerySecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entitydocquery/EntityDocQuerySecured.java"/>
        </condition>
    </target>
    <target name="wsimport-service-EntityDocQuerySecured" depends="wsimport-init,wsimport-service-check-EntityDocQuerySecured" unless="wsimport-service-EntityDocQuerySecured.notRequired">
        <property name="service-wsdl-EntityDocQuerySecured" location="${meta.inf}/xml-resources/web-services/EntityDocQuerySecured/wsdl/EntityDocQuerySecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-EntityDocQuerySecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-EntityDocQuerySecured" location="${meta.inf}/wsdl/EntityDocQuerySecured"/>
        <property name="sourceLocation-EntityDocQuerySecured" location="${meta.inf}/xml-resources/web-services/EntityDocQuerySecured/wsdl/"/>
        <copy todir="${targetLocation-EntityDocQuerySecured}">
            <fileset dir="${sourceLocation-EntityDocQuerySecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-EntityDocQuerySecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entitydocquery"/>
    </target>
    <target name="wsimport-service-check-NhincProxyDocQuerySecured" depends="wsimport-init">
        <condition property="wsimport-service-NhincProxyDocQuerySecured.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxydocquerysecured/NhincProxyDocQuerySecured.java"/>
        </condition>
    </target>
    <target name="wsimport-service-NhincProxyDocQuerySecured" depends="wsimport-init,wsimport-service-check-NhincProxyDocQuerySecured" unless="wsimport-service-NhincProxyDocQuerySecured.notRequired">
        <property name="service-wsdl-NhincProxyDocQuerySecured" location="${meta.inf}/xml-resources/web-services/NhincProxyDocQuerySecured/wsdl/NhincProxyDocQuerySecured.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-NhincProxyDocQuerySecured}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-NhincProxyDocQuerySecured" location="${meta.inf}/wsdl/NhincProxyDocQuerySecured"/>
        <property name="sourceLocation-NhincProxyDocQuerySecured" location="${meta.inf}/xml-resources/web-services/NhincProxyDocQuerySecured/wsdl/"/>
        <copy todir="${targetLocation-NhincProxyDocQuerySecured}">
            <fileset dir="${sourceLocation-NhincProxyDocQuerySecured}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincProxyDocQuerySecured" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxydocquerysecured"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-EntityDocQuerySecured, wsimport-service-NhincProxyDocQuerySecured"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
