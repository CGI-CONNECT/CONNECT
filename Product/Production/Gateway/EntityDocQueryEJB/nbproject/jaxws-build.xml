<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-check-EntityDocQuery" depends="wsimport-init">
        <condition property="wsimport-service-EntityDocQuery.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entitydocquery/EntityDocQuery.java"/>
        </condition>
    </target>
    <target name="wsimport-service-EntityDocQuery" depends="wsimport-init,wsimport-service-check-EntityDocQuery" unless="wsimport-service-EntityDocQuery.notRequired">
        <property name="service-wsdl-EntityDocQuery" location="${meta.inf}/xml-resources/web-services/EntityDocQuery/wsdl/EntityDocQuery.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-EntityDocQuery}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-EntityDocQuery" location="${meta.inf}/wsdl/EntityDocQuery"/>
        <property name="sourceLocation-EntityDocQuery" location="${meta.inf}/xml-resources/web-services/EntityDocQuery/wsdl/"/>
        <copy todir="${targetLocation-EntityDocQuery}">
            <fileset dir="${sourceLocation-EntityDocQuery}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-EntityDocQuery" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/entitydocquery"/>
    </target>
    <target name="wsimport-service-check-NhincProxyDocQuery" depends="wsimport-init">
        <condition property="wsimport-service-NhincProxyDocQuery.notRequired">
            <available file="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxydocquery/NhincProxyDocQuery.java"/>
        </condition>
    </target>
    <target name="wsimport-service-NhincProxyDocQuery" depends="wsimport-init,wsimport-service-check-NhincProxyDocQuery" unless="wsimport-service-NhincProxyDocQuery.notRequired">
        <property name="service-wsdl-NhincProxyDocQuery" location="${meta.inf}/xml-resources/web-services/NhincProxyDocQuery/wsdl/NhincProxyDocQuery.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/service" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${service-wsdl-NhincProxyDocQuery}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true"/>
        <property name="targetLocation-NhincProxyDocQuery" location="${meta.inf}/wsdl/NhincProxyDocQuery"/>
        <property name="sourceLocation-NhincProxyDocQuery" location="${meta.inf}/xml-resources/web-services/NhincProxyDocQuery/wsdl/"/>
        <copy todir="${targetLocation-NhincProxyDocQuery}">
            <fileset dir="${sourceLocation-NhincProxyDocQuery}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NhincProxyDocQuery" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/gov/hhs/fha/nhinc/nhincproxydocquery"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-EntityDocQuery, wsimport-service-NhincProxyDocQuery"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
