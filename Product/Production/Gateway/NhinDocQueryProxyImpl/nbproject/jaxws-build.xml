<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-NhinDocQuery" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/NhinDocQuery"/>
        <property name="wsdl-NhinDocQuery" location="xml-resources/web-service-references/NhinDocQuery/wsdl/NhinDocQuery.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/NhinDocQuery" destdir="${build.generated.dir}/jax-wsCache/NhinDocQuery" wsdl="${wsdl-NhinDocQuery}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/NhinDocQuery.wsdl" xnocompile="true">
            <depends file="${wsdl-NhinDocQuery}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/NhinDocQuery"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/NhinDocQuery">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-NhinDocQuery" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/NhinDocQuery"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/ihe:iti:xds-b:2007"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-NhinDocQuery"/>
</project>
