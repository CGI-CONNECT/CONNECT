<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-service-HardCodeTestService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/HardCodeTestService"/>
        <property name="service-wsdl-HardCodeTestService" location="${meta.inf}/xml-resources/web-services/HardCodeTestService/wsdl/HardCodePathTest.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/HardCodeTestService" destdir="${build.generated.dir}/jax-wsCache/service/HardCodeTestService" wsdl="${service-wsdl-HardCodeTestService}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-HardCodeTestService}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/HardCodeTestService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/HardCodeTestService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-HardCodeTestService" location="${meta.inf}/wsdl/HardCodeTestService"/>
        <property name="sourceLocation-HardCodeTestService" location="${meta.inf}/xml-resources/web-services/HardCodeTestService/wsdl/"/>
        <copy todir="${targetLocation-HardCodeTestService}">
            <fileset dir="${sourceLocation-HardCodeTestService}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-HardCodeTestService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/HardCodeTestService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/hardcodepathtest"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-HardCodeTestService"/>
</project>
