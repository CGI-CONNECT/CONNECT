<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-HardCodePathTest" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/HardCodePathTest"/>
        <property name="wsdl-HardCodePathTest" location="${meta.inf}/xml-resources/web-service-references/HardCodePathTest/wsdl/HardCodePathTest.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/HardCodePathTest" destdir="${build.generated.dir}/jax-wsCache/HardCodePathTest" wsdl="${wsdl-HardCodePathTest}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="file:/C:/projects/NHINC/Current/Product/Production/Common/Interfaces/src/wsdl/HardCodePathTest.wsdl" xnocompile="true">
            <depends file="${wsdl-HardCodePathTest}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/HardCodePathTest"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/HardCodePathTest">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-HardCodePathTest" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/HardCodePathTest"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov:hhs:fha:nhinc:hardcodepathtest"/>
    </target>
    <target name="wsimport-service-HardCodeTest2Service" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/HardCodeTest2Service"/>
        <property name="service-wsdl-HardCodeTest2Service" location="${meta.inf}/xml-resources/web-services/HardCodeTest2Service/wsdl/HardCodePathTest2.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/HardCodeTest2Service" destdir="${build.generated.dir}/jax-wsCache/service/HardCodeTest2Service" wsdl="${service-wsdl-HardCodeTest2Service}" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${wsdl-HardCodeTest2Service}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/HardCodeTest2Service"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/HardCodeTest2Service">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <property name="targetLocation-HardCodeTest2Service" location="${meta.inf}/wsdl/HardCodeTest2Service"/>
        <property name="sourceLocation-HardCodeTest2Service" location="${meta.inf}/xml-resources/web-services/HardCodeTest2Service/wsdl/"/>
        <copy todir="${targetLocation-HardCodeTest2Service}">
            <fileset dir="${sourceLocation-HardCodeTest2Service}"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-HardCodeTest2Service" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/service/HardCodeTest2Service"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/hardcodepathtest2"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-HardCodePathTest"/>
    <target name="wsimport-service-generate" depends="wsimport-service-HardCodeTest2Service"/>
</project>
