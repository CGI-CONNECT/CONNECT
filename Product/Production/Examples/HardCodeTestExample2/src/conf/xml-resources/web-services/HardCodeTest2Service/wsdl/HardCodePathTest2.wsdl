<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" 
						 xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
						 xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
						 xmlns:tns="urn:gov:hhs:fha:nhinc:hardcodepathtest2"
						 xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" 
						 xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
                                                 xmlns:nccommon="urn:gov:hhs:fha:nhinc:common:nhinccommon"
						 name="HardCodePathTest"
						 targetNamespace="urn:gov:hhs:fha:nhinc:hardcodepathtest2">
    <wsdl:types>
        <xsd:schema>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommon" 
                        schemaLocation="../schemas/nhinc/common/NhincCommon.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <message name="Test2RequestMessage">
        <part name="Test2Request"
              element="nccommon:PersonName"/>
    </message>
    <message name="Test2ResponseMessage">
        <part name="Test2Response"
              element="nccommon:PersonName"/>
    </message>
    <portType name="HardCodeTest2PortType">
        <operation name="Test2">
            <input name="Test2Request"
                   message="tns:Test2RequestMessage"/>
            <output name="Test2Response"
                    message="tns:Test2ResponseMessage"/>
        </operation>
    </portType>
    <binding name="HardCodeTest2BindingSoap11"
             type="tns:HardCodeTest2PortType">
        <soap:binding style="document" 
                      transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="Test2">
            <soap:operation soapAction="urn:Test2"/>
            <input name="Test2Request">
                <soap:body use="literal"/>
            </input>
            <output name="Test2Response">
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>
    <service name="HardCodeTest2">
        <port name="HardCodeTest2PortSoap11"
              binding="tns:HardCodeTest2BindingSoap11">
            <soap:address 
              location="http://localhost:${NhincHttpPort}/NhinConnect/HardCodeTest2"/>
        </port>
    </service>
    <plnk:partnerLinkType name="HardCodeTest2">
		<!-- A partner link type is automatically generated when a new port 
             type is added. Partner link types are used by BPEL processes. 
             In a BPEL process, a partner link represents the interaction 
             between the BPEL process and a partner service. Each partner 
             link is associated with a partner link type.  A partner link 
             type characterizes the conversational relationship between 
             two services. The partner link type can have one or two roles.-->
        <plnk:role name="HardCodeTest2PortTypeRole"
                   portType="tns:HardCodeTest2PortType"/>
    </plnk:partnerLinkType>
</definitions>
