<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:ejbjarproject2="http://www.netbeans.org/ns/j2ee-ejbjarproject/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSGEN SECTION
                ===================
            -->
    <target name="wsgen-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsgen/service"/>
        <mkdir dir="${build.generated.dir}/wsgen/service/resources/"/>
        <mkdir dir="${build.generated.dir}/wsgen/binaries"/>
        <taskdef name="wsgen" classname="com.sun.tools.ws.ant.WsGen">
            <classpath path="${j2ee.platform.wsgen.classpath}"/>
        </taskdef>
    </target>
    <target name="wsgen-compile" depends="wsgen-UtilityService">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsgen/service" classpath="${j2ee.platform.wsgen.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
    <target name="wsgen-UtilityService" depends="wsgen-init, -do-compile">
        <wsgen xendorsed="true" fork="true" destdir="${build.generated.dir}/wsgen/binaries" sourcedestdir="${build.generated.dir}/wsgen/service" resourcedestdir="${build.generated.dir}/wsgen/service/resources/" keep="false" genwsdl="true" sei="nhinc.wsn.Utility">
            <classpath path="${java.home}/../lib/tools.jar:${classes.dir}:${j2ee.platform.wsgen.classpath}:${javac.classpath}"/>
            <jvmarg value="-Djava.endorsed.dirs=${jaxws.endorsed.dir}"/>
        </wsgen>
    </target>
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <mkdir dir="${build.generated.dir}/wsimport/service"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <mkdir dir="${classes.dir}"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${j2ee.platform.wsimport.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-check-NhinSubscription" depends="wsimport-init">
        <condition property="wsimport-client-NhinSubscription.notRequired">
            <available file="${build.generated.dir}/wsimport/client/org/oasis_open/docs/wsn/bw_2" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-client-NhinSubscription" depends="wsimport-init,wsimport-client-check-NhinSubscription" unless="wsimport-client-NhinSubscription.notRequired">
        <wsimport xendorsed="true" sourcedestdir="${build.generated.dir}/wsimport/client" extension="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${meta.inf}/xml-resources/web-service-references/NhinSubscription/wsdl/NhinSubscription.wsdl" wsdlLocation="file:/C:/Projects/NHINC/Current/Product/Production/Examples/WSN/Interfaces/src/WSDL/NhinSubscription.wsdl" catalog="catalog.xml"/>
        <copy todir="${classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-client-clean-NhinSubscription" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/org/oasis_open/docs/wsn/bw_2"/>
    </target>
    <target name="wsimport-client-check-SubscriptionSink" depends="wsimport-init">
        <condition property="wsimport-client-SubscriptionSink.notRequired">
            <available file="${build.generated.dir}/wsimport/client/org/netbeans/j2ee/wsdl/interfaces/subscriptionsink" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-client-SubscriptionSink" depends="wsimport-init,wsimport-client-check-SubscriptionSink" unless="wsimport-client-SubscriptionSink.notRequired">
        <wsimport xendorsed="true" sourcedestdir="${build.generated.dir}/wsimport/client" extension="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${meta.inf}/xml-resources/web-service-references/SubscriptionSink/wsdl/SubscriptionSink.wsdl" wsdlLocation="file:/C:/Projects/NHINC/Current/Product/Production/Examples/WSN/Interfaces/src/WSDL/SubscriptionSink.wsdl" catalog="catalog.xml"/>
        <copy todir="${classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-client-clean-SubscriptionSink" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/org/netbeans/j2ee/wsdl/interfaces/subscriptionsink"/>
    </target>
    <target name="wsimport-service-check-NotificationProducerService" depends="wsimport-init">
        <condition property="wsimport-service-NotificationProducerService.notRequired">
            <available file="${build.generated.dir}/wsimport/service/org/oasis_open/docs/wsn/bw_2" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-service-NotificationProducerService" depends="wsimport-init,wsimport-service-check-NotificationProducerService" unless="wsimport-service-NotificationProducerService.notRequired">
        <wsimport xendorsed="true" sourcedestdir="${build.generated.dir}/wsimport/service" extension="true" verbose="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${meta.inf}/xml-resources/web-services/NotificationProducerService/wsdl/NhinSubscription.wsdl" catalog="catalog.xml"/>
        <copy todir="${basedir}/${meta.inf}/wsdl/NotificationProducerService">
            <fileset dir="${basedir}/${meta.inf}/xml-resources/web-services/NotificationProducerService/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NotificationProducerService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/org/oasis_open/docs/wsn/bw_2"/>
    </target>
    <target name="wsimport-service-check-NotificationConsumerService" depends="wsimport-init">
        <condition property="wsimport-service-NotificationConsumerService.notRequired">
            <available file="${build.generated.dir}/wsimport/service/org/oasis_open/docs/wsn/bw_2" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-service-NotificationConsumerService" depends="wsimport-init,wsimport-service-check-NotificationConsumerService" unless="wsimport-service-NotificationConsumerService.notRequired">
        <wsimport xendorsed="true" sourcedestdir="${build.generated.dir}/wsimport/service" extension="true" verbose="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${meta.inf}/xml-resources/web-services/NotificationConsumerService/wsdl/NhinSubscription.wsdl" catalog="catalog.xml"/>
        <copy todir="${basedir}/${meta.inf}/wsdl/NotificationConsumerService">
            <fileset dir="${basedir}/${meta.inf}/xml-resources/web-services/NotificationConsumerService/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-NotificationConsumerService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/service/org/oasis_open/docs/wsn/bw_2"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-NhinSubscription, wsimport-client-SubscriptionSink"/>
    <target name="wsimport-client-compile" depends="wsimport-client-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-NotificationProducerService, wsimport-service-NotificationConsumerService, wsimport-init"/>
    <target name="wsimport-service-compile" depends="wsimport-service-generate">
        <ejbjarproject2:javac srcdir="${build.generated.dir}/wsimport/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${classes.dir}"/>
    </target>
</project>
