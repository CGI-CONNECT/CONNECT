<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE dashboard [
	<!ENTITY % entities SYSTEM "..\Entities.xml">

	%entities;
]>
<dashboard>
	<remoteServices>
		<servers>
      <server name="&ProjectName;-&ProjectCodeLineName; Integration Test" url="tcp://192.168.25.157:&Port;/CruiseManager.rem" />
      <server name="&ProjectName;-&ProjectCodeLineName;" url="tcp://&HostName;:&Port;/CruiseManager.rem" />
    </servers>
	</remoteServices>
	<plugins>
		<farmPlugins>
			<farmReportFarmPlugin />
			<cctrayDownloadPlugin />
		</farmPlugins>
		<serverPlugins>
			<serverReportServerPlugin />
			<serverLogServerPlugin />
			<serverInformationServerPlugin />
		</serverPlugins>
		<projectPlugins>
			<projectReportProjectPlugin />
			<latestBuildReportProjectPlugin />
			<viewAllBuildsProjectPlugin />
		</projectPlugins>
		<buildPlugins>
			<buildReportBuildPlugin imageFileName="images/BuildReport.gif">
				<xslFileNames>
          <xslFile>Packages\Analytics\Basic\SatisticsSummary.xsl</xslFile>
          <xslFile>Packages\Alerts\AlertsSummary.xsl</xslFile>
					<xslFile>xsl\header.xsl</xslFile>
					<xslFile>Packages\TargetProcess\TargetProcess.xsl</xslFile>
					<xslFile>xsl\modifications.xsl</xslFile>
					<xslFile>Packages\SourceModificationReport\modifications.xsl</xslFile>
					<xslFile>Packages\Ant\compile-ant.xsl</xslFile>
					<xslFile>xsl\compile.xsl</xslFile>
					<xslFile>Packages\Deployment\deployment.xsl</xslFile>
					<xslFile>Packages\JUnit\JUnitDisplaySummary.xsl</xslFile>
					<xslFile>Packages\Simian\SimianDisplaySummary.xsl</xslFile>
				</xslFileNames>
			</buildReportBuildPlugin>

			<buildLogBuildPlugin imageFileName="images/Log.gif"/>
			<xslReportBuildPlugin description="NAnt Output" actionName="NAntOutputBuildReport" xslFileName="xsl\Nant.xsl" imageFileName="images/NAntOutput.gif" />
			<xslReportBuildPlugin description="NAnt Timings" actionName="NAntTimingsBuildReport" xslFileName="xsl\NantTiming.xsl" imageFileName="images/NAntTimings.gif"/>
			<artifactXslBuildPlugin description="Post Build" actionName="PostBuildNAntOutputBuildReport" xslFileName="xsl\Nant.xsl" artifactRootUrl="/&ProjectName;-&ProjectCodeLineName;/Artifacts" xmlFileName="postbuildlog.xml" imageFileName="images/NAntOutput.gif"/>
			<artifactViewBuildPlugin description="Artifact View" actionName="ArtifactView" artifactRootUrl="/&ProjectName;-&ProjectCodeLineName;/Artifacts" imageFileName="images/Artifacts.gif"/>
			<artifactXslBuildPlugin description="Diff"              actionName="DiffReports"                xslFileName="Packages\CSDiff\CSDiffReport.xsl"    artifactRootUrl="/&ProjectName;-&ProjectCodeLineName;/Artifacts" xmlFileName="postbuildlog.xml" imageFileName="Packages\CSDiff\Diff.gif" />
      <artifactXslBuildPlugin description="Unit Test Details" actionName="UnitTestDetailsBuildReport" xslFileName="Packages\JUnit\junit-noframes.xsl"   artifactRootUrl="/&ProjectName;-&ProjectCodeLineName;/Artifacts" xmlFileName="unittests.xml"    imageFileName="Packages\JUnit\logo.ico" />
			<xslReportBuildPlugin description="Ant Report" actionName="AntReport" xslFileName="Packages\Ant\AntReport.xsl" imageFileName="Packages\Ant\Logo.ico" />
			<xslReportBuildPlugin description="Ant Timings" actionName="AntTimingsBuildReport" xslFileName="Packages\Ant\AntTiming.xsl" imageFileName="images/NAntTimings.gif"/>
			<artifactXslBuildPlugin description="Analytics Report" actionName="AnalyticsReport" xslFileName="Packages\Analytics\Basic\statistics.xsl" artifactRootUrl="/&ProjectName;-&ProjectCodeLineName;/Artifacts" xmlFileName="MainAnalyticsReport.xml"  imageFileName="Packages\Analytics\Analytics.gif" />
			<xslReportBuildPlugin description="Simian Report" actionName="SimianReport" xslFileName="Packages\Simian\SimianDisplayReport.xsl" imageFileName="Packages\Simian\logo.gif" />
		</buildPlugins>
	</plugins>
</dashboard>